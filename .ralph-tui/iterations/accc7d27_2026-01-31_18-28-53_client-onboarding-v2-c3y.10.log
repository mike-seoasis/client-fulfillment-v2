ion_factory = async_sessionmaker(\n    92→                bind=self._engine,\n    93→                class_=AsyncSession,\n    94→                expire_on_commit=False,\n    95→                autoflush=False,\n    96→            )\n    97→\n    98→            # Set up pool event listeners for monitoring\n    99→            self._setup_pool_listeners()\n   100→\n   101→            logger.info(\"Database engine initialized successfully\")\n   102→\n   103→        except Exception as e:\n   104→            db_logger.connection_error(e, db_url)\n   105→            raise\n   106→\n   107→    def _setup_pool_listeners(self) -> None:\n   108→        \"\"\"Set up SQLAlchemy pool event listeners.\"\"\"\n   109→        if self._engine is None:\n   110→            return\n   111→\n   112→        # Pool listeners are registered on the Pool class globally\n   113→        # sync_engine reference not needed since we listen to Pool class directly\n   114→\n   115→        @event.listens_for(Pool, \"checkout\")\n   116→        def on_checkout(\n   117→            dbapi_conn: Any, connection_record: Any, connection_proxy: Any\n   118→        ) -> None:\n   119→            \"\"\"Track connection checkouts for pool monitoring.\"\"\"\n   120→            connection_record.info[\"checkout_time\"] = time.monotonic()\n   121→\n   122→        @event.listens_for(Pool, \"checkin\")\n   123→        def on_checkin(dbapi_conn: Any, connection_record: Any) -> None:\n   124→            \"\"\"Clear checkout time on checkin.\"\"\"\n   125→            connection_record.info.pop(\"checkout_time\", None)\n   126→\n   127→    async def close(self) -> None:\n   128→        \"\"\"Close database connections.\"\"\"\n   129→        if self._engine is not None:\n   130→            await self._engine.dispose()\n   131→            self._engine = None\n   132→            self._session_factory = None\n   133→            logger.info(\"Database connections closed\")\n   134→\n   135→    async def check_connection(s