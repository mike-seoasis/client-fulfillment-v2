  259→        priority=30,\n   260→        description=\"Shopping cart and checkout pages\",\n   261→    ),\n   262→    # Search pages\n   263→    CategoryRule(\n   264→        category=\"search\",\n   265→        patterns=[\n   266→            r\"^/search/?\",  # /search or /search?q=...\n   267→            r\"^/s\\?\",  # Short search /s?q=...\n   268→            r\"^/find/?\",  # /find?q=...\n   269→        ],\n   270→        priority=20,\n   271→        description=\"Search results pages\",\n   272→    ),\n   273→]\n   274→\n   275→\n   276→class URLCategorizer:\n   277→    \"\"\"Categorizes URLs based on configurable pattern rules.\n   278→\n   279→    Uses a priority-based system where higher priority rules are evaluated first.\n   280→    Falls back to 'other' category if no rules match.\n   281→\n   282→    Example usage:\n   283→        categorizer = URLCategorizer()\n   284→\n   285→        # Categorize a URL\n   286→        category, matched_rule = categorizer.categorize(\"https://example.com/products/widget\")\n   287→        # Returns: (\"product\", \"/products?/[^/]+/?$\")\n   288→\n   289→        # Categorize with page context\n   290→        category = categorizer.categorize_page(\n   291→            url=\"https://example.com/\",\n   292→            title=\"Welcome to Example Store\",\n   293→        )\n   294→        # Returns: \"homepage\"\n   295→    \"\"\"\n   296→\n   297→    def __init__(\n   298→        self,\n   299→        rules: list[CategoryRule] | None = None,\n   300→        default_category: str = \"other\",\n   301→    ) -> None:\n   302→        \"\"\"Initialize the categorizer with rules.\n   303→\n   304→        Args:\n   305→            rules: List of CategoryRule objects. Uses DEFAULT_CATEGORY_RULES if None.\n   306→            default_category: Category to assign when no rules match.\n   307→        \"\"\"\n   308→        logger.debug(\n   309→            \"URLCategorizer.__init__ called\",\n   310→            extra={\n   311→                \"rule_count\": len(rules) if rules else len(DEFAULT_CATEGORY_RULES),\n   312→                \"default_category\": default_category,\n   313→            },\n   314→        )\n   315→\n   316→        self._rules = rules if rules is not None else DEFAULT_CATEGORY_RULES.copy()\n   317→        self._default_category = default_category\n   318→\n   319→        # Sort rules by priority (highest first)\n   320→        self._rules.sort(key=lambda r: r.priority, reverse=True)\n   321→\n   322→        logger.debug(\n   323→            \"URLCategorizer initialized\",\n   324→            extra={\n   325→                \"rule_count\": len(self._rules),\n   326→                \"categories\": list({r.category for r in self._rules}),\n   327→            },\n   328→        )\n   329→\n   330→    @property\n   331→    def rules(self) -> list[CategoryRule]:\n   332→        \"\"\"Get the categorization rules (read-only copy).\"\"\"\n   333→        return self._rules.copy()\n   334→\n   335→    @property\n   336→    def default_category(self) -> str:\n   337→        \"\"\"Get the default category.\"\"\"\n   338→        return self._default_category\n   339→\n   340→    @property\n   341→    def categories(self) -> set[str]:\n   342→        \"\"\"Get all available categories.\"\"\"\n   343→        categories = {r.category for r in self._rules}\n   344→        categories.add(self._default_category)\n   345→        return categories\n   346→\n   347→    def add_rule(self, rule: CategoryRule) -> None:\n   348→        \"\"\"Add a new categorization rule.\n   349→\n   350→        Args:\n   351→            rule: The CategoryRule to add.\n   352→        \"\"\"\n   353→        logger.debug(\n   354→            \"Adding category rule\",\n   355→            extra={\n   356→                \"category\": rule.category,\n   357→                \"pattern_count\": len(rule.patterns),\n   358→                \"priority\": rule.priority,\n   359→            },\n   360→        )\n   361→\n   362→        self._rules.append(rule)\n   363→        # Re-sort by priority\n   364→        self._rules.sort(key=lambda r: r.priority, reverse=True)\n   365→\n   366→    def categorize(self, url: str) -> tuple[str, str | None]:\n   367→        \"\"\"Categorize a URL based on its path pattern.\n   368→\n   369→        Args:\n   370→            url: The URL to categorize (full URL or just path).\n   371→\n   372→        Returns:\n   373→            Tuple of (category, matched_pattern or None)\n   374→        \"\"\"\n   375→        start_time = time.monotonic()\n   376→        logger.debug(\n   377→            \"categorize() called\",\n   378→            extra={\"url\": url[:200] if url else \"\"},\n   379→        )\n   380→\n   381→        if not url or not url.strip():\n   382→            logger.warning(\n   383→                \"Categorization failed: empty URL\",\n   384→                extra={\"field\": \"url\", \"rejected_value\": repr(url)},\n   385→            )\n   386→            return self._default_category, None\n   387→\n   388→        try:\n   389→            # Extract path from URL\n   390→            parsed = urlparse(url.strip())\n   391→            path = parsed.path or \"/\"\n   392→\n   393→            # Normalize path (remove trailing slash except for root)\n   394→            if len(path) > 1 and path.endswith(\"/\"):\n   395→                path = path.rstrip(\"/\")\n   396→\n   397→            # Check each rule in priority order\n   398→            for rule in self._rules:\n   399→                matches, pattern = rule.matches(path)\n   400→                if matches:\n   401→                    duration_ms = (time.monotonic() - start_time) * 1000\n   402→                    logger.debug(\n   403→                        \"URL categorized\",\n   404→                        extra={\n   405→                            \"url\": url[:200],\n   406→                            \"path\": path,\n   407→                            \"category\": rule.category,\n   408→                            \"matched_pattern\": pattern,\n   409→                            \"duration_ms\": round(duration_ms, 2),\n   410→                        },\n   411→                    )\n   412→\n   413→                    if duration_ms > SLOW_OPERATION_THRESHOLD_MS:\n   414→                        logger.warning(\n   415→                            \"Slow URL categorization\",\n   416→                            extra={\n   417→                                \"url\": url[:200],\n   418→                                \"duration_ms\": round(duration_ms, 2),\n   419→                            },\n   420→                        )\n   421→\n   422→                    return rule.category, pattern\n   423→\n   424→            # No rule matched\n   425→            duration_ms = (time.monotonic() - start_time) * 1000\n   426→            logger.debug(\n   427→                \"URL categorized as default\",\n   428→                extra={\n   429→                    \"url\": url[:200],\n   430→                    \"path\": path,\n   431→                    \"category\": self._default_category,\n   432→                    \"duration_ms\": round(duration_ms, 2),\n   433→                },\n   434→            )\n   435→\n   436→            return self._default_category, None\n   437→\n   438→        except Exception as e:\n   439→            logger.error(\n   440→                \"Categorization failed with exception\",\n   441→                extra={\n   442→                    \"url\": url[:200] if url else \"\",\n   443→                    \"error_type\": type(e).__name__,\n   444→                    \"error_message\": str(e),\n   445→                },\n   446→                exc_info=True,\n   447→            )\n   448→            return self._default_category, None\n   449→\n   450→    def categorize_page(\n   451→        self,\n   452→        url: str,\n   453→        title: str | None = None,\n   454→        content_hints: dict[str, Any] | None = None,\n   455→    ) -> str:\n   456→        \"\"\"Categorize a page using URL and optional additional context.\n   457→\n   458→        This method extends basic URL categorization with optional page context\n   459→        like title and content hints. Currently uses URL-based categorization\n   460→        with potential for future enhancements using title/content analysis.\n   461→\n   462→        Args:\n   463→            url: The page URL to categorize.\n   464→            title: Optional page title (for future enhancement).\n   465→            content_hints: Optional content hints dict (for future enhancement).\n   466→\n   467→        Returns:\n   468→            The category string.\n   469→        \"\"\"\n   470→        logger.debug(\n   471→            \"categorize_page() called\",\n   472→            extra={\n   473→                \"url\": url[:200] if url else \"\",\n   474→                \"has_title\": title is not None,\n   475→                \"has_content_hints\": content_hints is not None,\n   476→            },\n   477→        )\n   478→\n   479→        # Currently uses URL-based categorization\n   480→        # Future: could incorporate title/content analysis\n   481→        category, _ = self.categorize(url)\n   482→\n   483→        # Log additional context for debugging\n   484→        if title:\n   485→            logger.debug(\n   486→                \"Page categorized with context\",\n   487→                extra={\n   488→                    \"url\": url[:200] if url else \"\",\n   489→                    \"title\": title[:100] if title else None,\n   490→                    \"category\": category,\n   491→                },\n   492→            )\n   493→\n   494→        return category\n   495→\n   496→    def categorize_many(\n   497→        self,\n   498→        urls: list[str],\n   499→    ) -> dict[str, str]:\n   500→        \"\"\"Categorize multiple URLs efficiently.\n   501→\n   502→        Args:\n   503→            urls: List of URLs to categorize.\n   504→\n   505→        Returns:\n   506→            Dict mapping URL to category.\n   507→        \"\"\"\n   508→        start_time = time.monotonic()\n   509→        logger.debug(\n   510→            \"categorize_many() called\",\n   511→            extra={\"url_count\": len(urls)},\n   512→        )\n   513→\n   514→        results: dict[str, str] = {}\n   515→        category_counts: dict[str, int] = {}\n   516→\n   517→        for url in urls:\n   518→            category, _ = self.categorize(url)\n   519→            results[url] = category\n   520→            category_counts[category] = category_counts.get(category, 0) + 1\n   521→\n   522→        duration_ms = (time.monotonic() - start_time) * 1000\n   523→        logger.debug(\n   524→            \"categorize_many() completed\",\n   525→            extra={\n   526→                \"url_count\": len(urls),\n   527→                \"category_counts\": category_counts,\n   528→                \"duration_ms\": round(duration_ms, 2),\n   529→            },\n   530→        )\n   531→\n   532→        if duration_ms > SLOW_OPERATION_THRESHOLD_MS:\n   533→            logger.warning(\n   534→                \"Slow batch categorization\",\n   535→                extra={\n   536→                    \"url_count\": len(urls),\n   537→                    \"duration_ms\": round(duration_ms, 2),\n   538→                },\n   539→            )\n   540→\n   541→        return results\n   542→\n   543→    def get_rule_for_category(self, category: str) -> CategoryRule | None:\n   544→        \"\"\"Get the rule definition for a category.\n   545→\n   546→        Args:\n   547→            category: The category to look up.\n   548→\n   549→        Returns:\n   550→            CategoryRule or None if not found.\n   551→        \"\"\"\n   552→        for rule in self._rules:\n   553→            if rule.category == category:\n   554→                return rule\n   555→        return None\n   556→\n   557→\n   558→# Default instance for convenience\n   559→_default_categorizer: URLCategorizer | None = None\n   560→\n   561→\n   562→def get_url_categorizer() -> URLCategorizer:\n   563→    \"\"\"Get the default URLCategorizer instance (singleton).\n   564→\n   565→    Returns:\n   566→        Default URLCategorizer instance.\n   567→    \"\"\"\n   568→    global _default_categorizer\n   569→    if _default_categorizer is None:\n   570→        _default_categorizer = URLCategorizer()\n   571→    return _default_categorizer\n   572→\n   573→\n   574→def categorize_url(url: str) -> str:\n   575→    \"\"\"Convenience function to categorize a URL.\n   576→\n   577→    Args:\n   578→        url: The URL to categorize.\n   579→\n   580→    Returns:\n   581→        The category string.\n   582→\n   583→    Example:\n   584→        >>> categorize_url(\"https://example.com/products/widget\")\n   585→        'product'\n   586→        >>> categorize_url(\"https://example.com/blog/my-post\")\n   587→        'blog'\n   588→        >>> categorize_url(\"https://example.com/privacy-policy\")\n   589→        'policy'\n   590→    \"\"\"\n   591→    categorizer = get_url_categorizer()\n   592→    category, _ = categorizer.categorize(url)\n   593→    return category\n   594→\n   595→\n   596→# Valid categories constant for validation\n   597→VALID_PAGE_CATEGORIES = frozenset({\n   598→    \"homepage\",\n   599→    \"product\",\n   600→    \"collection\",\n   601→    \"blog\",\n   602→    \"policy\",\n   603→    \"about\",\n   604→    \"contact\",\n   605→    \"faq\",\n   606→    \"account\",\n   607→    \"cart\",\n   608→    \"search\",\n   609→    \"other\",\n   610→})\n   611→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"bdedd681-d2dd-4275-8492-8fa10ed0b74e","uuid":"79bf9410-221b-44ac-a36a-e8442fd40fc5","tool_use_result":{"type":"text","file":{"filePath":"/Users/mike/Projects/client-onboarding-v2/backend/app/utils/url_categorizer.py","content":"\"\"\"URL pattern rules for categorizing crawled pages.\n\nProvides intelligent URL categorization based on common URL patterns:\n- Collection pages (/collections/*, /category/*, /shop/*)\n- Product pages (/products/*, /product/*, /p/*)\n- Blog pages (/blog/*, /news/*, /articles/*)\n- Policy pages (/policy/*, /policies/*, /legal/*, /terms/*, /privacy/*)\n- Homepage (/)\n- About pages (/about/*, /about-us/*)\n- Contact pages (/contact/*, /contact-us/*)\n- FAQ/Help pages (/faq/*, /help/*, /support/*)\n\nERROR LOGGING REQUIREMENTS:\n- Log method entry/exit at DEBUG level with parameters (sanitized)\n- Log all exceptions with full stack trace and context\n- Include entity IDs (project_id, page_id) in all service logs\n- Log validation failures with field names and rejected values\n- Log state transitions (phase changes) at INFO level\n- Add timing logs for operations >1 second\n\"\"\"\n\nimport re\nimport time\nfrom dataclasses import dataclass\nfrom typing import Any\nfrom urllib.parse import urlparse\n\nfrom app.core.logging import get_logger\n\nlogger = get_logger(\"url_categorizer\")\n\n# Threshold for logging slow operations (in milliseconds)\nSLOW_OPERATION_THRESHOLD_MS = 1000\n\n\n@dataclass\nclass CategoryRule:\n    \"\"\"A single URL pattern rule for categorization.\n\n    Attributes:\n        category: The category name to assign (e.g., 'product', 'collection')\n        patterns: List of regex patterns that match this category\n        priority: Higher priority rules are evaluated first (default: 0)\n        description: Human-readable description of the rule\n    \"\"\"\n\n    category: str\n    patterns: list[str]\n    priority: int = 0\n    description: str = \"\"\n\n    def __post_init__(self) -> None:\n        \"\"\"Compile regex patterns for efficiency.\"\"\"\n        self._compiled_patterns: list[re.Pattern[str]] = []\n        for pattern in self.patterns:\n            try:\n                self._compiled_patterns.append(re.compile(pattern, re.IGNORECASE))\n            except re.error as e:\n                logger.warning(\n                    \"Invalid regex pattern in category rule\",\n                    extra={\n                        \"category\": self.category,\n                        \"pattern\": pattern,\n                        \"error\": str(e),\n                    },\n                )\n\n    def matches(self, path: str) -> tuple[bool, str | None]:\n        \"\"\"Check if a URL path matches any of this rule's patterns.\n\n        Args:\n            path: The URL path to check\n\n        Returns:\n            Tuple of (matches, matched_pattern)\n        \"\"\"\n        for i, compiled in enumerate(self._compiled_patterns):\n            if compiled.search(path):\n                return True, self.patterns[i]\n        return False, None\n\n\n# Default category rules for common website patterns\n# Ordered by priority (higher = checked first)\nDEFAULT_CATEGORY_RULES: list[CategoryRule] = [\n    # Homepage - highest priority, exact match\n    CategoryRule(\n        category=\"homepage\",\n        patterns=[\n            r\"^/?$\",  # Exact root\n            r\"^/index\\.html?$\",  # index.html or index.htm\n            r\"^/home/?$\",  # /home or /home/\n        ],\n        priority=100,\n        description=\"Homepage/landing page\",\n    ),\n    # Product pages - common ecommerce patterns\n    CategoryRule(\n        category=\"product\",\n        patterns=[\n            r\"^/products?/[^/]+/?$\",  # /product/item or /products/item\n            r\"^/p/[^/]+/?$\",  # Short product URL /p/item\n            r\"^/shop/[^/]+/[^/]+/?$\",  # /shop/category/item\n            r\"^/item/[^/]+/?$\",  # /item/item-name\n            r\"^/merchandise/[^/]+/?$\",  # /merchandise/item\n            r\"^/sku/[^/]+/?$\",  # /sku/12345\n        ],\n        priority=80,\n        description=\"Individual product pages\",\n    ),\n    # Collection/category pages - lists of products\n    CategoryRule(\n        category=\"collection\",\n        patterns=[\n            r\"^/collections?/[^/]+/?$\",  # /collection/summer or /collections/summer\n            r\"^/collections?/?$\",  # /collections (listing all)\n            r\"^/categor(y|ies)/[^/]*/?$\",  # /category/shoes or /categories/shoes\n            r\"^/shop/[^/]+/?$\",  # /shop/category (but not /shop/category/item)\n            r\"^/shop/?$\",  # /shop main page\n            r\"^/browse/[^/]*/?$\",  # /browse/category\n            r\"^/catalog/[^/]*/?$\",  # /catalog/category\n            r\"^/store/[^/]*/?$\",  # /store/category\n        ],\n        priority=70,\n        description=\"Collection/category listing pages\",\n    ),\n    # Blog/content pages\n    CategoryRule(\n        category=\"blog\",\n        patterns=[\n            r\"^/blog/?$\",  # Blog listing\n            r\"^/blog/[^/]+/?$\",  # Individual blog post\n            r\"^/blog/category/[^/]+/?$\",  # Blog category\n            r\"^/blog/tag/[^/]+/?$\",  # Blog tag\n            r\"^/news/?$\",  # News listing\n            r\"^/news/[^/]+/?$\",  # Individual news article\n            r\"^/articles?/?$\",  # Articles listing\n            r\"^/articles?/[^/]+/?$\",  # Individual article\n            r\"^/posts?/[^/]+/?$\",  # /post/slug or /posts/slug\n            r\"^/stories?/[^/]+/?$\",  # /story/slug or /stories/slug\n            r\"^/insights?/[^/]*/?$\",  # /insights or /insights/article\n            r\"^/resources?/[^/]*/?$\",  # /resources or /resources/article\n        ],\n        priority=60,\n        description=\"Blog and content pages\",\n    ),\n    # Policy/legal pages - important for compliance\n    CategoryRule(\n        category=\"policy\",\n        patterns=[\n            r\"^/polic(y|ies)/[^/]*/?$\",  # /policy/privacy or /policies/terms\n            r\"^/privacy-policy/?$\",  # /privacy-policy\n            r\"^/privacy/?$\",  # /privacy\n            r\"^/terms-of-service/?$\",  # /terms-of-service\n            r\"^/terms-and-conditions/?$\",  # /terms-and-conditions\n            r\"^/terms/?$\",  # /terms\n            r\"^/tos/?$\",  # /tos\n            r\"^/legal/?$\",  # /legal\n            r\"^/legal/[^/]+/?$\",  # /legal/privacy\n            r\"^/refund-policy/?$\",  # /refund-policy\n            r\"^/refund/?$\",  # /refund\n            r\"^/shipping-policy/?$\",  # /shipping-policy\n            r\"^/shipping/?$\",  # /shipping (if just info page)\n            r\"^/returns?(-policy)?/?$\",  # /return or /returns or /return-policy\n            r\"^/cookie-policy/?$\",  # /cookie-policy\n            r\"^/cookies?/?$\",  # /cookie or /cookies\n            r\"^/gdpr/?$\",  # /gdpr\n            r\"^/ccpa/?$\",  # /ccpa\n            r\"^/accessibility/?$\",  # /accessibility\n            r\"^/disclaimer/?$\",  # /disclaimer\n        ],\n        priority=50,\n        description=\"Policy and legal pages\",\n    ),\n    # About pages\n    CategoryRule(\n        category=\"about\",\n        patterns=[\n            r\"^/about(-us)?/?$\",  # /about or /about-us\n            r\"^/about/[^/]+/?$\",  # /about/team\n            r\"^/company/?$\",  # /company\n            r\"^/team/?$\",  # /team\n            r\"^/our-story/?$\",  # /our-story\n            r\"^/who-we-are/?$\",  # /who-we-are\n            r\"^/mission/?$\",  # /mission\n            r\"^/values/?$\",  # /values\n            r\"^/history/?$\",  # /history\n        ],\n        priority=40,\n        description=\"About and company pages\",\n    ),\n    # Contact pages\n    CategoryRule(\n        category=\"contact\",\n        patterns=[\n            r\"^/contact(-us)?/?$\",  # /contact or /contact-us\n            r\"^/get-in-touch/?$\",  # /get-in-touch\n            r\"^/reach-us/?$\",  # /reach-us\n            r\"^/locations?/?$\",  # /location or /locations\n            r\"^/store-locator/?$\",  # /store-locator\n            r\"^/find-us/?$\",  # /find-us\n        ],\n        priority=40,\n        description=\"Contact and location pages\",\n    ),\n    # FAQ/Help pages\n    CategoryRule(\n        category=\"faq\",\n        patterns=[\n            r\"^/faq/?$\",  # /faq\n            r\"^/faqs?/[^/]*/?$\",  # /faq/category or /faqs/\n            r\"^/help/?$\",  # /help\n            r\"^/help/[^/]+/?$\",  # /help/topic\n            r\"^/help-center/?$\",  # /help-center\n            r\"^/support/?$\",  # /support\n            r\"^/support/[^/]+/?$\",  # /support/topic\n            r\"^/knowledge-base/?$\",  # /knowledge-base\n            r\"^/kb/[^/]*/?$\",  # /kb/article\n            r\"^/how-to/[^/]*/?$\",  # /how-to/guide\n            r\"^/guides?/[^/]*/?$\",  # /guide/topic or /guides/topic\n            r\"^/tutorials?/[^/]*/?$\",  # /tutorial/topic or /tutorials/topic\n        ],\n        priority=35,\n        description=\"FAQ and help pages\",\n    ),\n    # Account/auth pages\n    CategoryRule(\n        category=\"account\",\n        patterns=[\n            r\"^/account/?$\",  # /account\n            r\"^/account/[^/]+/?$\",  # /account/settings\n            r\"^/my-account/?$\",  # /my-account\n            r\"^/login/?$\",  # /login\n            r\"^/signin/?$\",  # /signin\n            r\"^/sign-in/?$\",  # /sign-in\n            r\"^/register/?$\",  # /register\n            r\"^/signup/?$\",  # /signup\n            r\"^/sign-up/?$\",  # /sign-up\n            r\"^/forgot-password/?$\",  # /forgot-password\n            r\"^/reset-password/?$\",  # /reset-password\n            r\"^/profile/?$\",  # /profile\n            r\"^/dashboard/?$\",  # /dashboard\n            r\"^/orders?/?$\",  # /order or /orders\n            r\"^/wishlist/?$\",  # /wishlist\n        ],\n        priority=30,\n        description=\"Account