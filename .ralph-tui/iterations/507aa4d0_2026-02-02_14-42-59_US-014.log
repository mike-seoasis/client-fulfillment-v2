with the database record ID\n  1104→                result.brief_id = brief.id\n  1105→\n  1106→                logger.info(\n  1107→                    \"Content brief fetched and saved successfully\",\n  1108→                    extra={\n  1109→                        \"project_id\": project_id,\n  1110→                        \"page_id\": page_id,\n  1111→                        \"brief_id\": brief.id,\n  1112→                        \"keyword\": keyword[:50],\n  1113→                        \"task_id\": result.task_id,\n  1114→                    },\n  1115→                )\n  1116→\n  1117→            except POPContentBriefServiceError:\n  1118→                # Re-raise persistence errors\n  1119→                raise\n  1120→            except Exception as e:\n  1121→                logger.error(\n  1122→                    \"Failed to save content brief after successful fetch\",\n  1123→                    extra={\n  1124→                        \"project_id\": project_id,\n  1125→                        \"page_id\": page_id,\n  1126→                        \"keyword\": keyword[:50],\n  1127→                        \"error\": str(e),\n  1128→                        \"error_type\": type(e).__name__,\n  1129→                        \"stack_trace\": traceback.format_exc(),\n  1130→                    },\n  1131→                    exc_info=True,\n  1132→                )\n  1133→                raise POPContentBriefServiceError(\n  1134→                    f\"Failed to save content brief: {e}\",\n  1135→                    project_id=project_id,\n  1136→                    page_id=page_id,\n  1137→                ) from e\n  1138→\n  1139→        elif result.success and self._session is None:\n  1140→            logger.warning(\n  1141→                \"Content brief fetched but not saved - no database session\",\n  1142→                extra={\n  1143→                    \"project_id\": project_id,\n  1144→                    \"page_id\": page_id,\n  1145→   