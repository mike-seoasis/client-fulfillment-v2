o create project\",\n   154→                extra={\n   155→                    \"name\": data.name,\n   156→                    \"client_id\": data.client_id,\n   157→                    \"error_type\": type(e).__name__,\n   158→                    \"error_message\": str(e),\n   159→                },\n   160→                exc_info=True,\n   161→            )\n   162→            raise\n   163→\n   164→    async def get_project(self, project_id: str) -> Project:\n   165→        \"\"\"Get a project by ID.\n   166→\n   167→        Args:\n   168→            project_id: UUID of the project\n   169→\n   170→        Returns:\n   171→            Project instance\n   172→\n   173→        Raises:\n   174→            ProjectNotFoundError: If project is not found\n   175→        \"\"\"\n   176→        start_time = time.monotonic()\n   177→        logger.debug(\n   178→            \"Fetching project\",\n   179→            extra={\"project_id\": project_id},\n   180→        )\n   181→\n   182→        try:\n   183→            self._validate_uuid(project_id, \"project_id\")\n   184→\n   185→            project = await self.repository.get_by_id(project_id)\n   186→            if project is None:\n   187→                logger.debug(\n   188→                    \"Project not found\",\n   189→                    extra={\"project_id\": project_id},\n   190→                )\n   191→                raise ProjectNotFoundError(project_id)\n   192→\n   193→            duration_ms = (time.monotonic() - start_time) * 1000\n   194→            logger.debug(\n   195→                \"Project fetched successfully\",\n   196→                extra={\n   197→                    \"project_id\": project_id,\n   198→                    \"duration_ms\": round(duration_ms, 2),\n   199→                },\n   200→            )\n   201→\n   202→            if duration_ms > SLOW_OPERATION_THRESHOLD_MS:\n   203→                logger.warning(\n   204→                    \"Slow project fetch\",\n   205→   