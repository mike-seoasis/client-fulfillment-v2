ounts[\"include\"] += 1\n   615→            else:\n   616→                priority_counts[\"other\"] += 1\n   617→\n   618→        stats = {\n   619→            \"queue_size\": len(self._heap),\n   620→            \"seen_count\": len(self._seen),\n   621→            \"priority_counts\": priority_counts,\n   622→            \"start_url\": self._start_url,\n   623→            \"start_domain\": self._start_domain,\n   624→            \"include_patterns\": self._include_patterns,\n   625→            \"exclude_patterns\": self._exclude_patterns,\n   626→        }\n   627→\n   628→        crawl_queue_logger.queue_stats(\n   629→            len(self._heap),\n   630→            len(self._seen),\n   631→            priority_counts,\n   632→        )\n   633→\n   634→        return stats\n   635→\n   636→    def __iter__(self) -> Iterator[QueuedURL]:\n   637→        \"\"\"Iterate over queue in priority order (destructive - empties queue).\"\"\"\n   638→        while self._heap:\n   639→            item = self.pop()\n   640→            if item:\n   641→                yield item\n   642→\n   643→    def clear(self) -> None:\n   644→        \"\"\"Clear the queue (but keep seen set).\"\"\"\n   645→        logger.debug(\"clear() called\", extra={\"items_cleared\": len(self._heap)})\n   646→        self._heap.clear()\n   647→\n   648→    def reset(self) -> None:\n   649→        \"\"\"Reset the queue completely (clear queue and seen set).\"\"\"\n   650→        logger.debug(\n   651→            \"reset() called\",\n   652→            extra={\n   653→                \"queue_cleared\": len(self._heap),\n   654→                \"seen_cleared\": len(self._seen),\n   655→            },\n   656→        )\n   657→        self._heap.clear()\n   658→        self._seen.clear()\n   659→\n   660→        # Re-add start URL\n   661→        self._add_internal(\n   662→            url=self._start_url,\n   663→            normalized_url=self._normalized_start_url,\n   664→            priority=URLPri