e,\n   626→    ) -> None:\n   627→        \"\"\"Log failed API call at WARNING or ERROR level based on status.\"\"\"\n   628→        # 4xx at WARNING, 5xx and others at ERROR\n   629→        level = logging.WARNING if status_code and 400 <= status_code < 500 else logging.ERROR\n   630→        self.logger.log(\n   631→            level,\n   632→            f\"Claude API call failed: {model}\",\n   633→            extra={\n   634→                \"model\": model,\n   635→                \"duration_ms\": round(duration_ms, 2),\n   636→                \"status_code\": status_code,\n   637→                \"error\": error,\n   638→                \"error_type\": error_type,\n   639→                \"retry_attempt\": retry_attempt,\n   640→                \"request_id\": request_id,\n   641→                \"success\": False,\n   642→            },\n   643→        )\n   644→\n   645→    def timeout(self, model: str, timeout_seconds: float) -> None:\n   646→        \"\"\"Log request timeout at WARNING level.\"\"\"\n   647→        self.logger.warning(\n   648→            \"Claude API request timeout\",\n   649→            extra={\n   650→                \"model\": model,\n   651→                \"timeout_seconds\": timeout_seconds,\n   652→            },\n   653→        )\n   654→\n   655→    def rate_limit(\n   656→        self,\n   657→        model: str,\n   658→        retry_after: float | None = None,\n   659→        request_id: str | None = None,\n   660→    ) -> None:\n   661→        \"\"\"Log rate limit (429) at WARNING level.\"\"\"\n   662→        self.logger.warning(\n   663→            \"Claude API rate limit hit (429)\",\n   664→            extra={\n   665→                \"model\": model,\n   666→                \"retry_after_seconds\": retry_after,\n   667→                \"request_id\": request_id,\n   668→            },\n   669→        )\n   670→\n   671→    def auth_failure(self, status_code: int) -> None:\n   672→        \"\"\"Log authentication failure (4