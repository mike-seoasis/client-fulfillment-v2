\n   245→        \"\"\"Log Redis operation with timing.\"\"\"\n   246→        level = logging.DEBUG if success else logging.WARNING\n   247→        self.logger.log(\n   248→            level,\n   249→            f\"Redis {operation}\",\n   250→            extra={\n   251→                \"operation\": operation,\n   252→                \"key\": key[:100] if key else None,  # Truncate long keys\n   253→                \"duration_ms\": round(duration_ms, 2),\n   254→                \"success\": success,\n   255→                \"retry_attempt\": retry_attempt,\n   256→            },\n   257→        )\n   258→\n   259→    def timeout(self, operation: str, key: str, timeout_seconds: float) -> None:\n   260→        \"\"\"Log Redis timeout.\"\"\"\n   261→        self.logger.warning(\n   262→            \"Redis operation timeout\",\n   263→            extra={\n   264→                \"operation\": operation,\n   265→                \"key\": key[:100] if key else None,\n   266→                \"timeout_seconds\": timeout_seconds,\n   267→            },\n   268→        )\n   269→\n   270→    def circuit_state_change(\n   271→        self, previous_state: str, new_state: str, failure_count: int\n   272→    ) -> None:\n   273→        \"\"\"Log circuit breaker state change.\"\"\"\n   274→        self.logger.warning(\n   275→            \"Redis circuit breaker state changed\",\n   276→            extra={\n   277→                \"previous_state\": previous_state,\n   278→                \"new_state\": new_state,\n   279→                \"failure_count\": failure_count,\n   280→            },\n   281→        )\n   282→\n   283→    def circuit_open(self, failure_count: int, recovery_timeout: float) -> None:\n   284→        \"\"\"Log circuit breaker opening.\"\"\"\n   285→        self.logger.error(\n   286→            \"Redis circuit breaker opened - Redis operations disabled\",\n   287→            extra={\n   288→                \"failure_count\": failure_count,\n   289→          