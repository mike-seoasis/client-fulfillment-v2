t dataclass, field\n    45→from enum import Enum\n    46→from typing import Any\n    47→\n    48→import httpx\n    49→\n    50→from app.core.config import get_settings\n    51→from app.core.logging import get_logger\n    52→\n    53→logger = get_logger(__name__)\n    54→\n    55→\n    56→class CircuitState(Enum):\n    57→    \"\"\"Circuit breaker states.\"\"\"\n    58→\n    59→    CLOSED = \"closed\"  # Normal operation\n    60→    OPEN = \"open\"  # Failing, reject all requests\n    61→    HALF_OPEN = \"half_open\"  # Testing if service recovered\n    62→\n    63→\n    64→@dataclass\n    65→class CircuitBreakerConfig:\n    66→    \"\"\"Circuit breaker configuration.\"\"\"\n    67→\n    68→    failure_threshold: int\n    69→    recovery_timeout: float\n    70→\n    71→\n    72→class CircuitBreaker:\n    73→    \"\"\"Circuit breaker implementation for POP operations.\n    74→\n    75→    Prevents cascading failures by stopping requests to a failing service.\n    76→    After recovery_timeout, allows a test request through (half-open state).\n    77→    If test succeeds, circuit closes. If fails, circuit opens again.\n    78→    \"\"\"\n    79→\n    80→    def __init__(self, config: CircuitBreakerConfig) -> None:\n    81→        self._config = config\n    82→        self._state = CircuitState.CLOSED\n    83→        self._failure_count = 0\n    84→        self._last_failure_time: float | None = None\n    85→        self._lock = asyncio.Lock()\n    86→\n    87→    @property\n    88→    def state(self) -> CircuitState:\n    89→        \"\"\"Get current circuit state.\"\"\"\n    90→        return self._state\n    91→\n    92→    @property\n    93→    def is_closed(self) -> bool:\n    94→        \"\"\"Check if circuit is closed (normal operation).\"\"\"\n    95→        return self._state == CircuitState.CLOSED\n    96→\n    97→    @property\n    98→    def is_open(self) -> bool:\n    99→        \"\"\"Check if circuit is open (rejecting requests).\"\"\"\n   100→        return self._state == CircuitState.OPEN\n   101→\n   102→    async def _check_recovery(self) -> bool:\n   103→        \"\"\"Check if enough time has passed to attempt recovery.\"\"\"\n   104→        if self._last_failure_time is None:\n   105→            return False\n   106→        elapsed = time.monotonic() - self._last_failure_time\n   107→        return elapsed >= self._config.recovery_timeout\n   108→\n   109→    async def can_execute(self) -> bool:\n   110→        \"\"\"Check if operation can be executed based on circuit state.\"\"\"\n   111→        async with self._lock:\n   112→            if self._state == CircuitState.CLOSED:\n   113→                return True\n   114→\n   115→            if self._state == CircuitState.OPEN:\n   116→                if await self._check_recovery():\n   117→                    # Transition to half-open for testing\n   118→                    previous_state = self._state.value\n   119→                    self._state = CircuitState.HALF_OPEN\n   120→                    logger.warning(\n   121→                        \"POP circuit breaker state changed\",\n   122→                        extra={\n   123→                            \"previous_state\": previous_state,\n   124→                            \"new_state\": self._state.value,\n   125→                            \"failure_count\": self._failure_count,\n   126→                        },\n   127→                    )\n   128→                    logger.info(\"POP circuit breaker attempting recovery\")\n   129→                    return True\n   130→                return False\n   131→\n   132→            # HALF_OPEN state - allow single test request\n   133→            return True\n   134→\n   135→    async def record_success(self) -> None:\n   136→        \"\"\"Record successful operation.\"\"\"\n   137→        async with self._lock:\n   138→            if self._state == CircuitState.HALF_OPEN:\n   139→                # Recovery successful, close circuit\n   140→                previous_state = self._state.value\n   141→                self._state = CircuitState.CLOSED\n   142→                self._failure_count = 0\n   143→                self._last_failure_time = None\n   144→                logger.warning(\n   145→                    \"POP circuit breaker state changed\",\n   146→                    extra={\n   147→                        \"previous_state\": previous_state,\n   148→                        \"new_state\": self._state.value,\n   149→                        \"failure_count\": self._failure_count,\n   150→                    },\n   151→                )\n   152→                logger.info(\"POP circuit breaker closed - API calls restored\")\n   153→            elif self._state == CircuitState.CLOSED:\n   154→                # Reset failure count on success\n   155→                self._failure_count = 0\n   156→\n   157→    async def record_failure(self) -> None:\n   158→        \"\"\"Record failed operation.\"\"\"\n   159→        async with self._lock:\n   160→            self._failure_count += 1\n   161→            self._last_failure_time = time.monotonic()\n   162→\n   163→            if self._state == CircuitState.HALF_OPEN:\n   164→                # Recovery failed, open circuit again\n   165→                previous_state = self._state.value\n   166→                self._state = CircuitState.OPEN\n   167→                logger.warning(\n   168→                    \"POP circuit breaker state changed\",\n   169→                    extra={\n   170→                        \"previous_state\": previous_state,\n   171→                        \"new_state\": self._state.value,\n   172→                        \"failure_count\": self._failure_count,\n   173→                    },\n   174→                )\n   175→                logger.error(\n   176→                    \"POP circuit breaker opened - API calls disabled\",\n   177→                    extra={\n   178→                        \"failure_count\": self._failure_count,\n   179→                        \"recovery_timeout_seconds\": self._config.recovery_timeout,\n   180→                    },\n   181→                )\n   182→            elif (\n   183→                self._state == CircuitState.CLOSED\n   184→                and self._failure_count >= self._config.failure_threshold\n   185→            ):\n   186→                # Too many failures, open circuit\n   187→                previous_state = self._state.value\n   188→                self._state = CircuitState.OPEN\n   189→                logger.warning(\n   190→                    \"POP circuit breaker state changed\",\n   191→                    extra={\n   192→                        \"previous_state\": previous_state,\n   193→                        \"new_state\": self._state.value,\n   194→                        \"failure_count\": self._failure_count,\n   195→                    },\n   196→                )\n   197→                logger.error(\n   198→                    \"POP circuit breaker opened - API calls disabled\",\n   199→                    extra={\n   200→                        \"failure_count\": self._failure_count,\n   201→                        \"recovery_timeout_seconds\": self._config.recovery_timeout,\n   202→                    },\n   203→                )\n   204→\n   205→\n   206→class POPError(Exception):\n   207→    \"\"\"Base exception for POP API errors.\"\"\"\n   208→\n   209→    def __init__(\n   210→        self,\n   211→        message: str,\n   212→        status_code: int | None = None,\n   213→        response_body: dict[str, Any] | None = None,\n   214→        request_id: str | None = None,\n   215→    ) -> None:\n   216→        super().__init__(message)\n   217→        self.status_code = status_code\n   218→        self.response_body = response_body\n   219→        self.request_id = request_id\n   220→\n   221→\n   222→class POPTimeoutError(POPError):\n   223→    \"\"\"Raised when a request times out.\"\"\"\n   224→\n   225→    pass\n   226→\n   227→\n   228→class POPRateLimitError(POPError):\n   229→    \"\"\"Raised when rate limited (429).\"\"\"\n   230→\n   231→    def __init__(\n   232→        self,\n   233→        message: str,\n   234→        retry_after: float | None = None,\n   235→        response_body: dict[str, Any] | None = None,\n   236→        request_id: str | None = None,\n   237→    ) -> None:\n   238→        super().__init__(\n   239→            message, status_code=429, response_body=response_body, request_id=request_id\n   240→        )\n   241→        self.retry_after = retry_after\n   242→\n   243→\n   244→class POPAuthError(POPError):\n   245→    \"\"\"Raised when authentication fails (401/403).\"\"\"\n   246→\n   247→    pass\n   248→\n   249→\n   250→class POPCircuitOpenError(POPError):\n   251→    \"\"\"Raised when circuit breaker is open.\"\"\"\n   252→\n   253→    pass\n   254→\n   255→\n   256→class POPTaskStatus(Enum):\n   257→    \"\"\"POP task status values.\"\"\"\n   258→\n   259→    PENDING = \"pending\"\n   260→    PROCESSING = \"processing\"\n   261→    SUCCESS = \"success\"\n   262→    FAILURE = \"failure\"\n   263→    UNKNOWN = \"unknown\"\n   264→\n   265→\n   266→@dataclass\n   267→class POPTaskResult:\n   268→    \"\"\"Result of a POP task operation.\"\"\"\n   269→\n   270→    success: bool\n   271→    task_id: str | None = None\n   272→    status: POPTaskStatus = POPTaskStatus.UNKNOWN\n   273→    data: dict[str, Any] = field(default_factory=dict)\n   274→    error: str | None = None\n   275→    duration_ms: float = 0.0\n   276→    request_id: str | None = None\n   277→\n   278→\n   279→class POPClient:\n   280→    \"\"\"Async client for PageOptimizer Pro API.\n   281→\n   282→    Provides content optimization capabilities:\n   283→    - Content briefs with keyword targets\n   284→    - Content scoring and recommendations\n   285→    - Competitor analysis\n   286→\n   287→    Features:\n   288→    - Circuit breaker for fault tolerance\n   289→    - Retry logic with exponential backoff\n   290→    - Comprehensive logging\n   291→    - Railway deployment compatibility\n   292→    - Task polling for async operations\n   293→\n   294→    Authentication:\n   295→    POP uses apiKey in request body, not HTTP headers.\n   296→    All requests must include {\"apiKey\": \"<key>\"} in the JSON body.\n   297→    \"\"\"\n   298→\n   299→    def __init__(\n   300→        self,\n   301→        api_key: str | None = None,\n   302→        api_url: str | None = None,\n   303→        task_poll_interval: float | None = None,\n   304→        task_timeout: float | None = None,\n   305→        max_retries: int = 3,\n   306→        retry_delay: float = 1.0,\n   307→    ) -> None:\n   308→        \"\"\"Initialize POP client.\n   309→\n   310→        Args:\n   311→            api_key: POP API key. Defaults to settings.\n   312→            api_url: POP API base URL. Defaults to settings.\n   313→            task_poll_interval: Interval between task status polls. Defaults to settings.\n   314→            task_timeout: Maximum time to wait for task completion. Defaults to settings.\n   315→            max_retries: Maximum retry attempts. Defaults to 3.\n   316→            retry_delay: Base delay between retries. Defaults to 1.0.\n   317→        \"\"\"\n   318→        settings = get_settings()\n   319→\n   320→        self._api_key = api_key or settings.pop_api_key\n   321→        self._api_url = api_url or settings.pop_api_url\n   322→        self._task_poll_interval = task_poll_interval or settings.pop_task_poll_interval\n   323→        self._task_timeout = task_timeout or settings.pop_task_timeout\n   324→        self._max_retries = max_retries\n   325→        self._retry_delay = retry_delay\n   326→\n   327→        # Initialize circuit breaker\n   328→        self._circuit_breaker = CircuitBreaker(\n   329→            CircuitBreakerConfig(\n   330→                failure_threshold=settings.pop_circuit_failure_threshold,\n   331→                recovery_timeout=settings.pop_circuit_recovery_timeout,\n   332→            )\n   333→        )\n   334→\n   335→        # HTTP client (created lazily)\n   336→        self._client: httpx.AsyncClient | None = None\n   337→        self._available = bool(self._api_key)\n   338→\n   339→    @property\n   340→    def available(self) -> bool:\n   341→        \"\"\"Check if POP is configured and available.\"\"\"\n   342→        return self._available\n   343→\n   344→    @property\n   345→    def circuit_breaker(self) -> CircuitBreaker:\n   346→        \"\"\"Get the circuit breaker instance.\"\"\"\n   347→        return self._circuit_breaker\n   348→\n   349→    def _mask_api_key(self, body: dict[str, Any]) -> dict[str, Any]:\n   350→        \"\"\"Mask API key in body for logging.\"\"\"\n   351→        if \"apiKey\" in body:\n   352→            masked = body.copy()\n   353→            masked[\"apiKey\"] = \"****\"\n   354→            return masked\n   355→        return body\n   356→\n   357→    async def _get_client(self) -> httpx.AsyncClient:\n   358→        \"\"\"Get or create HTTP client.\"\"\"\n   359→        if self._client is None:\n   360→            self._client = httpx.AsyncClient(\n   361→                base_url=self._api_url,\n   362→                headers={\n   363→                    \"Accept\": \"application/json\",\n   364→                    \"Content-Type\": \"application/json\",\n   365→                },\n   366→                timeout=httpx.Timeout(self._task_timeout),\n   367→            )\n   368→        return self._client\n   369→\n   370→    async def close(self) -> None:\n   371→        \"\"\"Close the HTTP client.\"\"\"\n   372→        if self._client:\n   373→            await self._client.aclose()\n   374→            self._client = None\n   375→        logger.info(\"POP client closed\")\n   376→\n   377→    async def _make_request(\n   378→        self,\n   379→        endpoint: str,\n   380→        payload: dict[str, Any],\n   381→        method: str = \"POST\",\n   382→    ) -> tuple[dict[str, Any], str]:\n   383→        \"\"\"Make an HTTP request to POP API with retry logic.\n   384→\n   385→        Args:\n   386→            endpoint: API endpoint path\n   387→            payload: Request payload (apiKey will be added automatically)\n   388→            method: HTTP method\n   389→\n   390→        Returns:\n   391→            Tuple of (response_data, request_id)\n   392→\n   393→        Raises:\n   394→            POPError: On API errors\n   395→            POPTimeoutError: On timeout\n   396→            POPRateLimitError: On rate limit (429)\n   397→            POPAuthError: On auth failure (401/403)\n   398→            POPCircuitOpenError: When circuit breaker is open\n   399→        \"\"\"\n   400→        request_id = str(uuid.uuid4())[:8]\n   401→\n   402→        if not self._available:\n   403→            raise POPError(\n   404→                \"POP not configured (missing API key)\",\n   405→                request_id=request_id,\n   406→            )\n   407→\n   408→        if not await self._circuit_breaker.can_execute():\n   409→            logger.info(\n   410→                \"POP unavailable, circuit breaker open\",\n   411→                extra={\n   412→                    \"endpoint\": endpoint,\n   413→                    \"reason\": \"Circuit breaker open\",\n   414→                },\n   415→            )\n   416→            raise POPCircuitOpenError(\n   417→                \"Circuit breaker is open\",\n   418→                request_id=request_id,\n   419→            )\n   420→\n   421→        client = await self._get_client()\n   422→        last_error: Exception | None = None\n   423→\n   424→        # Add apiKey to payload\n   425→        request_payload = {\"apiKey\": self._api_key, **payload}\n   426→\n   427→        for attempt in range(self._max_retries):\n   428→            attempt_start = time.monotonic()\n   429→\n   430→            try:\n   431→                # Log request start (mask API key)\n   432→                logger.debug(\n   433→                    f\"POP API call: {method} {endpoint}\",\n   434→                    extra={\n   435→                        \"endpoint\": endpoint,\n   436→                        \"method\": method,\n   437→                        \"retry_attempt\": attempt,\n   438→                        \"request_id\": request_id,\n   439→                    },\n   440→                )\n   441→                logger.debug(\n   442→                    \"POP API request body\",\n   443→                    extra={\n   444→                        \"endpoint\": endpoint,\n   445→                        \"request_body\": self._mask_api_key(request_payload),\n   446→                    },\n   447→                )\n   448→\n   449→                # Make request\n   450→                response = await client.request(\n   451→                    method,\n   452→                    endpoint,\n   453→                    json=request_payload,\n   454→                )\n   455→                duration_ms = (time.monotonic() - attempt_start) * 1000\n   456→\n   457→                # Handle response based on status code\n   458→                if response.status_code == 429:\n   459→                    # Rate limited\n   460→                    retry_after_str = response.headers.get(\"retry-after\")\n   461→                    retry_after = float(retry_after_str) if retry_after_str else None\n   462→                    logger.warning(\n   463→                        \"POP API rate limit hit (429)\",\n   464→                        extra={\n   465→                            \"endpoint\": endpoint,\n   466→                            \"retry_after_seconds\": retry_after,\n   467→                            \"request_id\": request_id,\n   468→                        },\n   469→                    )\n   470→                    await self._circuit_breaker.record_failure()\n   471→\n   472→                    # If we have retry attempts left and Retry-After is reasonable\n   473→                    if (\n   474→                        attempt < self._max_retries - 1\n   475→                        and retry_after\n   476→                        and retry_after <= 60\n   477→                    ):\n   478→                        await asyncio.sleep(retry_after)\n   479→                        continue\n   480→\n   481→                    raise POPRateLimitError(\n   482→                        \"Rate limit exceeded\",\n   483→                        retry_after=retry_after,\n   484→                        request_id=request_id,\n   485→                    )\n   486→\n   487→                if response.status_code in (401, 403):\n   488→                    # Auth failure\n   489→                    logger.warning(\n   490→                        f\"POP API authentication failed ({response.status_code})\",\n   491→                        extra={\n   492→                            \"status_code\": response.status_code,\n   493→                        },\n   494→                    )\n   495→                    logger.warning(\n   496→                        f\"POP API call failed: {method} {endpoint}\",\n   497→                        extra={\n   498→                            \"endpoint\": endpoint,\n   499→                            \"method\": method,\n   500→                            \"duration_ms\": round(duration_ms, 2),\n   501→                            \"status_code\": response.status_code,\n   502→                            \"error\": \"Authentication failed\",\n   503→                            \"error_type\": \"AuthError\",\n   504→                            \"retry_attempt\": attempt,\n   505→                            \"request_id\": request_id,\n   506→                            \"success\": False,\n   507→                        },\n   508→                    )\n   509→                    await self._circuit_breaker.record_failure()\n   510→                    raise POPAuthError(\n   511→                        f\"Authentication failed ({response.status_code})\",\n   512→                        status_code=response.status_code,\n   513→                        request_id=request_id,\n   514→                    )\n   515→\n   516→                if response.status_code >= 500:\n   517→                    # Server error - retry\n   518→                    error_msg = f\"Server error ({response.status_code})\"\n   519→                    logger.error(\n   520→                        f\"POP API call failed: {method} {endpoint}\",\n   521→                        extra={\n   522→                            \"endpoint\": endpoint,\n   523→                            \"method\": method,\n   524→                            \"duration_ms\": round(duration_ms, 2),\n   525→                            \"status_code\": response.status_code,\n   526→                            \"error\": error_msg,\n   527→                            \"error_type\": \"ServerError\",\n   528→                            \"retry_attempt\": attempt,\n   529→                            \"request_id\": request_id,\n   530→                            \"success\": False,\n   531→                        },\n   532→                    )\n   533→                    await self._circuit_breaker.record_failure()\n   534→\n   535→                    if attempt < self._max_retries - 1:\n   536→                        delay = self._retry_delay * (2**attempt)\n   537→                        logger.warning(\n   538→                            f\"POP request attempt {attempt + 1} failed, \"\n   539→                            f\"retrying in {delay}s\",\n   540→                            extra={\n   541→                                \"attempt\": attempt + 1,\n   542→                                \"max_retries\": self._max_retries,\n   543→                                \"delay_seconds\": delay,\n   544→                                \"status_code\": response.status_code,\n   545→                                \"request_id\": request_id,\n   546→                            },\n   547→                        )\n   548→                        await asyncio.sleep(delay)\n   549→                        continue\n   550→\n   551→                    raise POPError(\n   552→                        error_msg,\n   553→                        status_code=response.status_code,\n   554→                        request_id=request_id,\n   555→                    )\n   556→\n   557→                if response.status_code >= 400:\n   558→                    # Client error - don't retry\n   559→                    error_body = response.json() if response.content else None\n   560→                    error_msg = str(error_body) if error_body else \"Client error\"\n   561→                    logger.warning(\n   562→                        f\"POP API call failed: {method} {endpoint}\",\n   563→                        extra={\n   564→                            \"endpoint\": endpoint,\n   565→                            \"method\": method,\n   566→                            \"duration_ms\": round(duration_ms, 2),\n   567→                            \"status_code\": response.status_code,\n   568→                            \"error\": error_msg,\n   569→                            \"error_type\": \"ClientError\",\n   570→                            \"retry_attempt\": attempt,\n   571→                            \"request_id\": request_id,\n   572→                            \"success\": False,\n   573→                        },\n   574→                    )\n   575→                    raise POPError(\n   576→                        f\"Client error ({response.status_code}): {error_msg}\",\n   577→                        status_code=response.status_code,\n   578→                        response_body=error_body,\n   579→                        request_id=request_id,\n   580→                    )\n   581→\n   582→                # Success - parse response\n   583→                response_data = response.json()\n   584→\n   585→                # Log success\n   586→                logger.debug(\n   587→                    f\"POP API call completed: {method} {endpoint}\",\n   588→                    extra={\n   589→                        \"endpoint\": endpoint,\n   590→                        \"method\": method,\n   591→                        \"duration_ms\": round(duration_ms, 2),\n   592→                        \"request_id\": request_id,\n   593→                        \"success\": True,\n   594→                    },\n   595→                )\n   596→\n   597→                await self._circuit_breaker.record_success()\n   598→\n   599→                return response_data, request_id\n   600→\n   601→            except httpx.TimeoutException:\n   602→                duration_ms = (time.monotonic() - attempt_start) * 1000\n   603→                logger.warning(\n   604→                    \"POP API request timeout\",\n   605→                    extra={\n   606→                        \"endpoint\": endpoint,\n   607→                        \"timeout_seconds\": self._task_timeout,\n   608→                    },\n   609→                )\n   610→                logger.error(\n   611→                    f\"POP API call failed: {method} {endpoint}\",\n   612→                    extra={\n   613→                        \"endpoint\": endpoint,\n   614→                        \"method\": method,\n   615→                        \"duration_ms\": round(duration_ms, 2),\n   616→                        \"status_code\": None,\n   617→                        \"error\": \"Request timed out\",\n   618→                        \"error_type\": \"TimeoutError\",\n   619→                        \"retry_attempt\": attempt,\n   620→                        \"request_id\": request_id,\n   621→                        \"success\": False,\n   622→                    },\n   623→                )\n   624→                await self._circuit_breaker.record_failure()\n   625→\n   626→                if attempt < self._max_retries - 1:\n   627→                    delay = self._retry_delay * (2**attempt)\n   628→                    logger.warning(\n   629→                        f\"POP request attempt {attempt + 1} timed out, \"\n   630→                        f\"retrying in {delay}s\",\n   631→                        extra={\n   632→                            \"attempt\": attempt + 1,\n   633→                            \"max_retries\": self._max_retries,\n   634→                            \"delay_seconds\": delay,\n   635→                        },\n   636→                    )\n   637→                    await asyncio.sleep(delay)\n   638→                    continue\n   639→\n   640→                last_error = POPTimeoutError(\n   641→                    f\"Request timed out after {self._task_timeout}s\",\n   642→                    request_id=request_id,\n   643→                )\n   644→\n   645→            except httpx.RequestError as e:\n   646→                duration_ms = (time.monotonic() - attempt_start) * 1000\n   647→                logger.error(\n   648→                    f\"POP API call failed: {method} {endpoint}\",\n   649→                    extra={\n   650→                        \"endpoint\": endpoint,\n   651→                        \"method\": method,\n   652→                        \"duration_ms\": round(duration_ms, 2),\n   653→                        \"status_code\": None,\n   654→                        \"error\": str(e),\n   655→                        \"error_type\": type(e).__name__,\n   656→                        \"retry_attempt\": attempt,\n   657→                        \"request_id\": request_id,\n   658→                        \"success\": False,\n   659→                    },\n   660→                )\n   661→                await self._circuit_breaker.record_failure()\n   662→\n   663→                if attempt < self._max_retries - 1:\n   664→                    delay = self._retry_delay * (2**attempt)\n   665→                    logger.warning(\n   666→                        f\"POP request attempt {attempt + 1} failed, \"\n   667→                        f\"retrying in {delay}s\",\n   668→                        extra={\n   669→                            \"attempt\": attempt + 1,\n   670→                            \"max_retries\": self._max_retries,\n   671→                            \"delay_seconds\": delay,\n   672→                            \"error\": str(e),\n   673→                        },\n   674→                    )\n   675→                    await asyncio.sleep(delay)\n   676→                    continue\n   677→\n   678→                last_error = POPError(\n   679→                    f\"Request failed: {e}\",\n   680→                    request_id=request_id,\n   681→                )\n   682→\n   683→        if last_error:\n   684→            raise last_error\n   685→        raise POPError(\n   686→            \"Request failed after all retries\",\n   687→            request_id=request_id,\n   688→        )\n   689→\n   690→    async def create_report_task(\n   691→        self,\n   692→        keyword: str,\n   693→        url: str,\n   694→    ) -> POPTaskResult:\n   695→        \"\"\"Create a POP report task for content scoring.\n   696→\n   697→        POSTs to the POP API to create a new content analysis task.\n   698→        The task runs asynchronously and must be polled for results.\n   699→\n   700→        Args:\n   701→            keyword: Target keyword for content optimization\n   702→            url: URL of the page to analyze\n   703→\n   704→        Returns:\n   705→            POPTaskResult with task_id if successful\n   706→        \"\"\"\n   707→        start_time = time.monotonic()\n   708→\n   709→        if not self._available:\n   710→            return POPTaskResult(\n   711→                success=False,\n   712→                error=\"POP not configured (missing API key)\",\n   713→            )\n   714→\n   715→        if not keyword or not url:\n   716→            return POPTaskResult(\n   717→                success=False,\n   718→                error=\"Both keyword and url are required\",\n   719→            )\n   720→\n   721→        logger.info(\n   722→            \"Creating POP report task\",\n   723→            extra={\n   724→                \"keyword\": keyword[:50],\n   725→                \"url\": url[:100],\n   726→            },\n   727→        )\n   728→\n   729→        payload = {\n   730→            \"keyword\": keyword,\n   731→            \"url\": url,\n   732→        }\n   733→\n   734→        try:\n   735→            response_data, request_id = await self._make_request(\n   736→                \"/api/report\",\n   737→                payload,\n   738→                method=\"POST\",\n   739→            )\n   740→\n   741→            # Extract task_id from response\n   742→            task_id = response_data.get(\"task_id\") or response_data.get(\"taskId\")\n   743→\n   744→            if not task_id:\n   745→                # Some APIs return the task_id directly or in a nested structure\n   746→                task_id = response_data.get(\"id\") or response_data.get(\"data\", {}).get(\n   747→                    \"task_id\"\n   748→                )\n   749→\n   750→            duration_ms = (time.monotonic() - start_time) * 1000\n   751→\n   752→            if task_id:\n   753→                logger.info(\n   754→                    \"POP report task created\",\n   755→                    extra={\n   756→                        \"task_id\": task_id,\n   757→                        \"keyword\": keyword[:50],\n   758→                        \"duration_ms\": round(duration_ms, 2),\n   759→                    },\n   760→                )\n   761→                return POPTaskResult(\n   762→                    success=True,\n   763→                    task_id=str(task_id),\n   764→                    status=POPTaskStatus.PENDING,\n   765→                    data=response_data,\n   766→                    duration_ms=duration_ms,\n   767→                    request_id=request_id,\n   768→                )\n   769→            else:\n   770→                logger.warning(\n   771→                    \"POP task created but no task_id returned\",\n   772→                    extra={\n   773→                        \"response_keys\": list(response_data.keys()),\n   774→                    },\n   775→                )\n   776→                return POPTaskResult(\n   777→                    success=False,\n   778→                    error=\"No task_id in response\",\n   779→                    data=response_data,\n   780→                    duration_ms=duration_ms,\n   781→                    request_id=request_id,\n   782→                )\n   783→\n   784→        except POPError as e:\n   785→            duration_ms = (time.monotonic() - start_time) * 1000\n   786→            logger.error(\n   787→                \"Failed to create POP report task\",\n   788→                extra={\n   789→                    \"keyword\": keyword[:50],\n   790→                    \"error\": str(e),\n   791→                    \"duration_ms\": round(duration_ms, 2),\n   792→                },\n   793→            )\n   794→            return POPTaskResult(\n   795→                success=False,\n   796→                error=str(e),\n   797→                duration_ms=duration_ms,\n   798→                request_id=e.request_id,\n   799→            )\n   800→\n   801→    async def get_task_result(\n   802→        self,\n   803→        task_id: str,\n   804→    ) -> POPTaskResult:\n   805→        \"\"\"Get the result of a POP task.\n   806→\n   807→        GETs the task status and results from the POP API.\n   808→\n   809→        Args:\n   810→            task_id: The task ID returned from create_report_task()\n   811→\n   812→        Returns:\n   813→            POPTaskResult with status and data if available\n   814→        \"\"\"\n   815→        start_time = time.monotonic()\n   816→\n   817→        if not self._available:\n   818→            return POPTaskResult(\n   819→                success=False,\n   820→                task_id=task_id,\n   821→                error=\"POP not configured (missing API key)\",\n   822→            )\n   823→\n   824→        if not task_id:\n   825→            return POPTaskResult(\n   826→                success=False,\n   827→                error=\"task_id is required\",\n   828→            )\n   829→\n   830→        logger.debug(\n   831→            \"Getting POP task result\",\n   832→            extra={\"task_id\": task_id},\n   833→        )\n   834→\n   835→        # POP API endpoint for task results\n   836→        # Note: This uses the app.pageoptimizer.pro domain per the notes\n   837→        endpoint = f\"/api/task/{task_id}/results/\"\n   838→\n   839→        try:\n   840→            # Note: GET request still needs apiKey in body for POP\n   841→            response_data, request_id = await self._make_request(\n   842→                endpoint,\n   843→                {},  # Empty payload, apiKey added by _make_request\n   844→                method=\"GET\",\n   845→            )\n   846→\n   847→            duration_ms = (time.monotonic() - start_time) * 1000\n   848→\n   849→            # Parse status from response\n   850→            status_str = (\n   851→                response_data.get(\"status\")\n   852→                or response_data.get(\"task_status\")\n   853→                or \"unknown\"\n   854→            )\n   855→            status_str = status_str.lower()\n   856→\n   857→            if status_str in (\"success\", \"complete\", \"completed\", \"done\"):\n   858→                status = POPTaskStatus.SUCCESS\n   859→            elif status_str in (\"failure\", \"failed\", \"error\"):\n   860→                status = POPTaskStatus.FAILURE\n   861→            elif status_str in (\"pending\", \"queued\", \"waiting\"):\n   862→                status = POPTaskStatus.PENDING\n   863→            elif status_str in (\"processing\", \"running\", \"in_progress\"):\n   864→                status = POPTaskStatus.PROCESSING\n   865→            else:\n   866→                status = POPTaskStatus.UNKNOWN\n   867→\n   868→            logger.debug(\n   869→                \"POP task status retrieved\",\n   870→                extra={\n   871→                    \"task_id\": task_id,\n   872→                    \"status\": status.value,\n   873→                    \"duration_ms\": round(duration_ms, 2),\n   874→                },\n   875→            )\n   876→\n   877→            return POPTaskResult(\n   878→                success=True,\n   879→                task_id=task_id,\n   880→                status=status,\n   881→                data=response_data,\n   882→                duration_ms=duration_ms,\n   883→                request_id=request_id,\n   884→            )\n   885→\n   886→        except POPError as e:\n   887→            duration_ms = (time.monotonic() - start_time) * 1000\n   888→            logger.error(\n   889→                \"Failed to get POP task result\",\n   890→                extra={\n   891→                    \"task_id\": task_id,\n   892→                    \"error\": str(e),\n   893→                    \"duration_ms\": round(duration_ms, 2),\n   894→                },\n   895→            )\n   896→            return POPTaskResult(\n   897→                success=False,\n   898→                task_id=task_id,\n   899→                error=str(e),\n   900→                duration_ms=duration_ms,\n   901→                request_id=e.request_id,\n   902→            )\n   903→\n   904→    async def poll_for_result(\n   905→        self,\n   906→        task_id: str,\n   907→        poll_interval: float | None = None,\n   908→        timeout: float | None = None,\n   909→    ) -> POPTaskResult:\n   910→        \"\"\"Poll for task completion with configurable interval and timeout.\n   911→\n   912→        Continuously polls get_task_result() until the task completes\n   913→        (SUCCESS or FAILURE) or the timeout is reached.\n   914→\n   915→        Args:\n   916→            task_id: The task ID to poll\n   917→            poll_interval: Seconds between polls. Defaults to settings (3s).\n   918→            timeout: Maximum seconds to wait. Defaults to settings (300s).\n   919→\n   920→        Returns:\n   921→            POPTaskResult with final status and data\n   922→\n   923→        Raises:\n   924→            POPTimeoutError: If timeout is reached before task completes\n   925→        \"\"\"\n   926→        poll_interval = poll_interval or self._task_poll_interval\n   927→        timeout = timeout or self._task_timeout\n   928→\n   929→        start_time = time.monotonic()\n   930→        poll_count = 0\n   931→\n   932→        logger.info(\n   933→            \"Starting POP task polling\",\n   934→            extra={\n   935→                \"task_id\": task_id,\n   936→                \"poll_interval_seconds\": poll_interval,\n   937→                \"timeout_seconds\": timeout,\n   938→            },\n   939→        )\n   940→\n   941→        while True:\n   942→            elapsed = time.monotonic() - start_time\n   943→\n   944→            # Check timeout\n   945→            if elapsed >= timeout:\n   946→                logger.error(\n   947→                    \"POP task polling timed out\",\n   948→                    extra={\n   949→                        \"task_id\": task_id,\n   950→                        \"elapsed_seconds\": round(elapsed, 2),\n   951→                        \"timeout_seconds\": timeout,\n   952→                        \"poll_count\": poll_count,\n   953→                    },\n   954→                )\n   955→                raise POPTimeoutError(\n   956→                    f\"Task {task_id} timed out after {elapsed:.1f}s\",\n   957→                    request_id=task_id,\n   958→                )\n   959→\n   960→            # Get current status\n   961→            result = await self.get_task_result(task_id)\n   962→            poll_count += 1\n   963→\n   964→            if not result.success:\n   965→                # API error - return the error result\n   966→                logger.warning(\n   967→                    \"POP task poll failed\",\n   968→                    extra={\n   969→                        \"task_id\": task_id,\n   970→                        \"error\": result.error,\n   971→                        \"poll_count\": poll_count,\n   972→                    },\n   973→                )\n   974→                return result\n   975→\n   976→            # Check for terminal states\n   977→            if result.status == POPTaskStatus.SUCCESS:\n   978→                logger.info(\n   979→                    \"POP task completed successfully\",\n   980→                    extra={\n   981→                        \"task_id\": task_id,\n   982→                        \"elapsed_seconds\": round(elapsed, 2),\n   983→                        \"poll_count\": poll_count,\n   984→                    },\n   985→                )\n   986→                return result\n   987→\n   988→            if result.status == POPTaskStatus.FAILURE:\n   989→                logger.warning(\n   990→                    \"POP task failed\",\n   991→                    extra={\n   992→                        \"task_id\": task_id,\n   993→                        \"elapsed_seconds\": round(elapsed, 2),\n   994→                        \"poll_count\": poll_count,\n   995→                        \"data\": result.data,\n   996→                    },\n   997→                )\n   998→                return result\n   999→\n  1000→            # Still processing - wait and poll again\n  1001→            logger.debug(\n  1002→                \"POP task still processing\",\n  1003→                extra={\n  1004→                    \"task_id\": task_id,\n  1005→                    \"status\": result.status.value,\n  1006→                    \"elapsed_seconds\": round(elapsed, 2),\n  1007→                    \"poll_count\": poll_count,\n  1008→                    \"next_poll_in\": poll_interval,\n  1009→                },\n  1010→            )\n  1011→\n  1012→            await asyncio.sleep(poll_interval)\n  1013→\n  1014→\n  1015→# Global POP client instance\n  1016→_pop_client: POPClient | None = None\n  1017→\n  1018→\n  1019→async def init_pop() -> POPClient:\n  1020→    \"\"\"Initialize the global POP client.\n  1021→\n  1022→    Returns:\n  1023→        Initialized POPClient instance\n  1024→    \"\"\"\n  1025→    global _pop_client\n  1026→    if _pop_client is None:\n  1027→        _pop_client = POPClient()\n  1028→        if _pop_client.available:\n  1029→            logger.info(\"POP client initialized\")\n  1030→        else:\n  1031→            logger.info(\"POP not configured (missing API key)\")\n  1032→    return _pop_client\n  1033→\n  1034→\n  1035→async def close_pop() -> None:\n  1036→    \"\"\"Close the global POP client.\"\"\"\n  1037→    global _pop_client\n  1038→    if _pop_client:\n  1039→        await _pop_client.close()\n  1040→        _pop_client = None\n  1041→\n  1042→\n  1043→async def get_pop_client() -> POPClient:\n  1044→    \"\"\"Dependency for getting POP client.\n  1045→\n  1046→    Usage:\n  1047→        @app.get(\"/content-score\")\n  1048→        async def score_content(\n  1049→            url: str,\n  1050→            client: POPClient = Depends(get_pop_client)\n  1051→        ):\n  1052→            result = await client.score_content(url)\n  1053→            ...\n  1054→    \"\"\"\n  1055→    global _pop_client\n  1056→    if _pop_client is None:\n  1057→        await init_pop()\n  1058→    return _pop_client  # type: ignore[return-value]\n  1059→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"parent_tool_use_id":null,"session_id":"70a39778-ec39-4283-b553-0cf83f218aab","uuid":"8b7162c6-808e-4c18-97a2-e716b1cd7cae","tool_use_result":{"type":"text","file":{"filePath":"/Users/mike/Projects/client-onboarding-v2/backend/app/integrations/pop.py","content":"\"\"\"PageOptimizer Pro (POP) API integration client for content scoring.\n\nFeatures:\n- Async HTTP client using httpx (direct API calls)\n- Circuit breaker for fault tolerance\n- Retry logic with exponential backoff\n- Request/response logging per requirements\n- Handles timeouts, rate limits (429), auth failures (401/403)\n- Masks API credentials in all logs\n- Task polling for async operations\n\nPageOptimizer Pro provides:\n- Content briefs with keyword targets\n- Content scoring and optimization recommendations\n- Competitor analysis\n- NLP-based content optimization\n\nERROR LOGGING REQUIREMENTS:\n- Log all outbound API calls with endpoint, method, timing\n- Log request/response bodies at DEBUG level (truncate large responses)\n- Log and handle: timeouts, rate limits (429), auth failures (401/403)\n- Include retry attempt number in logs\n- Mask API credentials in all logs\n- Log circuit breaker state changes\n\nRAILWAY DEPLOYMENT REQUIREMENTS:\n- All API credentials via environment variables (POP_API_KEY)\n- Never log or expose API credentials\n- Handle cold-start latency (first request may be slow)\n- Implement request timeouts (Railway has 5min request limit)\n\nAUTHENTICATION:\n- POP uses apiKey in request body (not HTTP headers)\n- All requests must include {\"apiKey\": \"<key>\"} in the JSON body\n\nAPI ENDPOINTS:\n- Task creation: POST to base URL with keyword/URL in body\n- Task results: GET https://app.pageoptimizer.pro/api/task/:task_id/results/\n\"\"\"\n\nimport asyncio\nimport time\nimport uuid\nfrom dataclasses import dataclass, field\nfrom enum import Enum\nfrom typing import Any\n\nimport httpx\n\nfrom app.core.config import get_settings\nfrom app.core.logging import get_logger\n\nlogger = get_logger(__name__)\n\n\nclass CircuitState(Enum):\n    \"\"\"Circuit breaker states.\"\"\"\n\n    CLOSED = \"closed\"  # Normal operation\n    OPEN = \"open\"  # Failing, reject all requests\n    HALF_OPEN = \"half_open\"  # Testing if service recovered\n\n\n@dataclass\nclass CircuitBreakerConfig:\n    \"\"\"Circuit breaker configuration.\"\"\"\n\n    failure_threshold: int\n    recovery_timeout: float\n\n\nclass CircuitBreaker:\n    \"\"\"Circuit breaker implementation for POP operations.\n\n    Prevents cascading failures by stopping requests to a failing service.\n    After recovery_timeout, allows a test request through (half-open state).\n    If test succeeds, circuit closes. If fails, circuit opens again.\n    \"\"\"\n\n    def __init__(self, config: CircuitBreakerConfig) -> None:\n        self._config = config\n        self._state = CircuitState.CLOSED\n        self._failure_count = 0\n        self._last_failure_time: float | None = None\n        self._lock = asyncio.Lock()\n\n    @property\n    def state(self) -> CircuitState:\n        \"\"\"Get current circuit state.\"\"\"\n        return self._state\n\n    @property\n    def is_closed(self) -> bool:\n        \"\"\"Check if circuit is closed (normal operation).\"\"\"\n        return self._state == CircuitState.CLOSED\n\n    @property\n    def is_open(self) -> bool:\n        \"\"\"Check if circuit is open (rejecting requests).\"\"\"\n        return self._state == CircuitState.OPEN\n\n    async def _check_recovery(self) -> bool:\n        \"\"\"Check if enough time has passed to attempt recovery.\"\"\"\n        if self._last_failure_time is None:\n            return False\n        elapsed = time.monotonic() - self._last_failure_time\n        return elapsed >= self._config.recovery_timeout\n\n    async def can_execute(self) -> bool:\n        \"\"\"Check if operation can be executed based on circuit state.\"\"\"\n        async with self._lock:\n            if self._state == CircuitState.CLOSED:\n                return True\n\n            if self._state == CircuitState.OPEN:\n                if await self._check_recovery():\n                    # Transition to half-open for testing\n                    previous_state = self._state.value\n                    self._state = CircuitState.HALF_OPEN\n                    logger.warning(\n                        \"POP circuit breaker state changed\",\n                        extra={\n                            \"previous_state\": previous_state,\n                            \"new_state\": self._state.value,\n                            \"failure_count\": self._failure_count,\n                        },\n                    )\n                    logger.info(\"POP circuit breaker attempting recovery\")\n                    return True\n                return False\n\n            # HALF_OPEN state - allow single test request\n            return True\n\n    async def record_success(self) -> None:\n        \"\"\"Record successful operation.\"\"\"\n        async with self._lock:\n            if self._state == CircuitState.HALF_OPEN:\n                # Recovery successful, close circuit\n                previous_state = self._state.value\n                self._state = CircuitState.CLOSED\n                self._failure_count = 0\n                self._last_failure_time = None\n                logger.warning(\n                    \"POP circuit breaker state changed\",\n                    extra={\n                        \"previous_state\": previous_state,\n                        \"new_state\": self._state.value,\n                        \"failure_count\": self._failure_count,\n                    },\n                )\n                logger.info(\"POP circuit breaker closed - API calls restored\")\n            elif self._state == CircuitState.CLOSED:\n                # Reset failure count on success\n                self._failure_count = 0\n\n    async def record_failure(self) -> None:\n        \"\"\"Record failed operation.\"\"\"\n        async with self._lock:\n            self._failure_count += 1\n            self._last_failure_time = time.monotonic()\n\n            if self._state == CircuitState.HALF_OPEN:\n                # Recovery failed, open circuit again\n                previous_state = self._state.value\n                self._state = CircuitState.OPEN\n                logger.warning(\n                    \"POP circuit breaker state changed\",\n                    extra={\n                        \"previous_state\": previous_state,\n                        \"new_state\": self._state.value,\n                        \"failure_count\": self._failure_count,\n                    },\n                )\n                logger.error(\n                    \"POP circuit breaker opened - API calls disabled\",\n                    extra={\n                        \"failure_count\": self._failure_count,\n                        \"recovery_timeout_seconds\": self._config.recovery_timeout,\n                    },\n                )\n            elif (\n                self._state == CircuitState.CLOSED\n                and self._failure_count >= self._config.failure_threshold\n            ):\n                # Too many failures, open circuit\n                previous_state = self._state.value\n                self._state = CircuitState.OPEN\n                logger.warning(\n                    \"POP circuit breaker state changed\",\n                    extra={\n                        \"previous_state\": previous_state,\n                        \"new_state\": self._state.value,\n                        \"failure_count\": self._failure_count,\n                    },\n                )\n                logger.error(\n                    \"POP circuit breaker opened - API calls disabled\",\n                    extra={\n                        \"failure_count\": self._failure_count,\n                        \"recovery_timeout_seconds\": self._config.recovery_timeout,\n                    },\n                )\n\n\nclass POPError(Exception):\n    \"\"\"Base exception for POP API errors.\"\"\"\n\n    def __init__(\n        self,\n        message: str,\n        status_code: int | None = None,\n        response_body: dict[str, Any] | None = None,\n        request_id: str | None = None,\n    ) -> None:\n        super().__init__(message)\n        self.status_code = status_code\n        self.response_body = response_body\n        self.request_id = request_id\n\n\nclass POPTimeoutError(POPError):\n    \"\"\"Raised when a request times out.\"\"\"\n\n    pass\n\n\nclass POPRateLimitError(POPError):\n    \"\"\"Raised when rate limited (429).\"\"\"\n\n    def __init__(\n        self,\n        message: str,\n        retry_after: float | None = None,\n        response_body: dict[str, Any] | None = None,\n        request_id: str | None = None,\n    ) -> None:\n        super().__init__(\n            message, status_code=429, response_body=response_body, request_id=request_id\n        )\n        self.retry_after = retry_after\n\n\nclass POPAuthError(POPError):\n    \"\"\"Raised when authentication fails (401/403).\"\"\"\n\n    pass\n\n\nclass POPCircuitOpenError(POPError):\n    \"\"\"Raised when circuit breaker is open.\"\"\"\n\n    pass\n\n\nclass POPTaskStatus(Enum):\n    \"\"\"POP task status values.\"\"\"\n\n    PENDING = \"pending\"\n    PROCESSING = \"processing\"\n    SUCCESS = \"success\"\n    FAILURE = \"failure\"\n    UNKNOWN = \"unknown\"\n\n\n@dataclass\nclass POPTaskResult:\n    \"\"\"Result of a POP task operation.\"\"\"\n\n    success: bool\n    task_id: str | None = None\n    status: POPTaskStatus = POPTaskStatus.UNKNOWN\n    data: dict[str, Any] = field(default_factory=dict)\n    error: str | None = None\n    duration_ms: float = 0.0\n    request_id: str | None = None\n\n\nclass POPClient:\n    \"\"\"Async client for PageOptimizer Pro API.\n\n    Provides content optimization capabilities:\n    - Content briefs with keyword targets\n    - Content scoring and recommendations\n    - Competitor analysis\n\n    Features:\n    - Circuit breaker for fault tolerance\n    - Retry logic with exponential backoff\n    - Comprehensive logging\n    - Railway deployment compatibility\n    - Task polling for async operations\n\n    Authentication:\n    POP uses apiKey in request body, not HTTP headers.\n    All requests must include {\"apiKey\": \"<key>\"} in the JSON body.\n    \"\"\"\n\n    def __init__(\n        self,\n        api_key: str | None = None,\n        api_url: str | None = None,\n        task_poll_interval: float | None = None,\n        task_timeout: float | None = None,\n        max_retries: int = 3,\n        retry_delay: float = 1.0,\n    ) -> None:\n        \"\"\"Initialize POP client.\n\n        Args:\n            api_key: POP API key. Defaults to settings.\n            api_url: POP API base URL. Defaults to settings.\n            task_poll_interval: Interval between task status polls. Defaults to settings.\n            task_timeout: Maximum time to wait for task completion. Defaults to settings.\n            max_retries: Maximum retry attempts. Defaults to 3.\n            retry_delay: Base delay between retries. Defaults to 1.0.\n        \"\"\"\n        settings = get_settings()\n\n        self._api_key = api_key or settings.pop_api_key\n        self._api_url = api_url or settings.pop_api_url\n        self._task_poll_interval = task_poll_interval or settings.pop_task_poll_interval\n        self._task_timeout = task_timeout or settings.pop_task_timeout\n        self._max_retries = max_retries\n        self._retry_delay = retry_delay\n\n        # Initialize circuit breaker\n        self._circuit_breaker = CircuitBreaker(\n            CircuitBreakerConfig(\n                failure_threshold=settings.pop_circuit_failure_threshold,\n                recovery_timeout=settings.pop_circuit_recovery_timeout,\n            )\n        )\n\n        # HTTP client (created lazily)\n        self._client: httpx.AsyncClient | None = None\n        self._available = bool(self._api_key)\n\n    @property\n    def available(self) -> bool:\n        \"\"\"Check if POP is configured and available.\"\"\"\n        return self._available\n\n    @property\n    def circuit_breaker(self) -> CircuitBreaker:\n        \"\"\"Get the circuit breaker instance.\"\"\"\n        return self._circuit_breaker\n\n    def _mask_api_key(self, body: dict[str, Any]) -> dict[str, Any]:\n        \"\"\"Mask API key in body for logging.\"\"\"\n        if \"apiKey\" in body:\n            masked = body.copy()\n            masked[\"apiKey\"] = \"****\"\n            return masked\n        return body\n\n    async def _get_client(self) -> httpx.AsyncClient:\n        \"\"\"Get or create HTTP client.\"\"\"\n        if self._client is None:\n            self._client = httpx.AsyncClient(\n                base_url=self._api_url,\n                headers={\n                    \"Accept\": \"application/json\",\n                    \"Content-Type\": \"application/json\",\n                },\n                timeout=httpx.Timeout(self._task_timeout),\n            )\n        return self._client\n\n    async def close(self) -> None:\n        \"\"\"Close the HTTP client.\"\"\"\n        if self._client:\n            await self._client.aclose()\n            self._client = None\n        logger.info(\"POP client closed\")\n\n    async def _make_request(\n        self,\n        endpoint: str,\n        payload: dict[str, Any],\n        method: str = \"POST\",\n    ) -> tuple[dict[str, Any], str]:\n        \"\"\"Make an HTTP request to POP API with retry logic.\n\n        Args:\n            endpoint: API endpoint path\n            payload: Request payload (apiKey will be added automatically)\n            method: HTTP method\n\n        Returns:\n            Tuple of (response_data, request_id)\n\n        Raises:\n            POPError: On API errors\n            POPTimeoutError: On timeout\n            POPRateLimitError: On rate limit (429)\n            POPAuthError: On auth failure (401/403)\n            POPCircuitOpenError: When circuit breaker is open\n        \"\"\"\n        request_id = str(uuid.uuid4())[:8]\n\n        if not self._available:\n            raise POPError(\n                \"POP not configured (missing API key)\",\n                request_id=request_id,\n            )\n\n        if not await self._circuit_breaker.can_execute():\n            logger.info(\n                \"POP unavailable, circuit breaker open\",\n                extra={\n                    \"endpoint\": endpoint,\n                    \"reason\": \"Circuit breaker open\",\n                },\n            )\n            raise POPCircuitOpenError(\n                \"Circuit breaker is open\",\n                request_id=request_id,\n            )\n\n        client = await self._get_client()\n        last_error: Exception | None = None\n\n        # Add apiKey to payload\n        request_payload = {\"apiKey\": self._api_key, **payload}\n\n        for attempt in range(self._max_retries):\n            attempt_start = time.monotonic()\n\n            try:\n                # Log request start (mask API key)\n                logger.debug(\n                    f\"POP API call: {method} {endpoint}\",\n                    extra={\n                        \"endpoint\": endpoint,\n                        \"method\": method,\n                        \"retry_attempt\": attempt,\n                        \"request_id\": request_id,\n                    },\n                )\n                logger.debug(\n                    \"POP API request body\",\n                    extra={\n                        \"endpoint\": endpoint,\n                        \"request_body\": self._mask_api_key(request_payload),\n                    },\n                )\n\n                # Make request\n                response = await client.request(\n                    method,\n                    endpoint,\n                    json=request_payload,\n                )\n                duration_ms = (time.monotonic() - attempt_start) * 1000\n\n                # Handle response based on status code\n                if response.status_code == 429:\n                    # Rate limited\n                    retry_after_str = response.headers.get(\"retry-after\")\n                    retry_after = float(retry_after_str) if retry_after_str else None\n                    logger.warning(\n                        \"POP API rate limit hit (429)\",\n                        extra={\n                            \"endpoint\": endpoint,\n                            \"retry_after_seconds\": retry_after,\n                            \"request_id\": request_id,\n                        },\n                    )\n                    await self._circuit_breaker.record_failure()\n\n                    # If we have retry attempts left and Retry-After is reasonable\n                    if (\n                        attempt < self._max_retries - 1\n                        and retry_after\n                        and retry_after <= 60\n                    ):\n                        await asyncio.sleep(retry_after)\n                        continue\n\n                    raise POPRateLimitError(\n                        \"Rate limit exceeded\",\n                        retry_after=retry_after,\n                        request_id=request_id,\n                    )\n\n                if response.status_code in (401, 403):\n                    # Auth failure\n                    logger.warning(\n                        f\"POP API authentication failed ({response.status_code})\",\n                        extra={\n                            \"status_code\": response.status_code,\n                        },\n                    )\n                    logger.warning(\n                        f\"POP API call failed: {method} {endpoint}\",\n                        extra={\n                            \"endpoint\": endpoint,\n                            \"method\": method,\n                            \"duration_ms\": round(duration_ms, 2),\n                            \"status_code\": response.status_code,\n                            \"error\": \"Authentication failed\",\n                            \"error_type\": \"AuthError\",\n                            \"retry_attempt\": attempt,\n                            \"request_id\": request_id,\n                            \"success\": False,\n                        },\n                    )\n                    await self._circuit_breaker.record_failure()\n                    raise POPAuthError(\n                        f\"Authentication failed ({response.status_code})\",\n                        status_code=response.status_code,\n                        request_id=request_id,\n                    )\n\n                if response.status_code >= 500:\n                    # Server error - retry\n                    error_msg = f\"Server error ({response.status_code})\"\n                    logger.error(\n                        f\"POP API call failed: {method} {endpoint}\",\n                        extra={\n                            \"endpoint\": endpoint,\n                            \"method\": method,\n                            \"duration_ms\": round(duration_ms, 2),\n                            \"status_code\": response.status_code,\n                            \"error\": error_msg,\n                            \"error_type\": \"ServerError\",\n                            \"retry_attempt\": attempt,\n                            \"request_id\": request_id,\n                            \"success\": False,\n                        },\n                    )\n                    await self._circuit_breaker.record_failure()\n\n                    if attempt < self._max_retries - 1:\n                        delay = self._retry_delay * (2**attempt)\n                        logger.warning(\n                            f\"POP request attempt {attempt + 1} failed, \"\n                            f\"retrying in {delay}s\",\n                            extra={\n                                \"attempt\": attempt + 1,\n                                \"max_retries\": self._max_retries,\n                                \"delay_seconds\": delay,\n                                \"status_code\": response.status_code,\n                                \"request_id\": request_id,\n                            },\n                        )\n                        await asyncio.sleep(delay)\n                        continue\n\n                    raise POPError(\n                        error_msg,\n                        status_code=response.status_code,\n                        request_id=request_id,\n                    )\n\n                if response.status_code >= 400:\n                    # Client error - don't retry\n                    error_body = response.json() if response.content else None\n                    error_msg = str(error_body) if error_body else \"Client error\"\n                    logger.warning(\n                        f\"POP API call failed: {method} {endpoint}\",\n                        extra={\n                            \"endpoint\": endpoint,\n                            \"method\": method,\n                            \"duration_ms\": round(duration_ms, 2),\n                            \"status_code\": response.status_code,\n                            \"error\": error_msg,\n                            \"error_type\": \"ClientError\",\n                            \"retry_attempt\": attempt,\n                            \"request_id\": request_id,\n                            \"success\": False,\n                        },\n                    )\n                    raise POPError(\n                        f\"Client error ({response.status_code}): {error_msg}\",\n                        status_code=response.status_code,\n                        response_body=error_body,\n                        request_id=request_id,\n                    )\n\n                # Success - parse response\n                response_data = response.json()\n\n                # Log success\n                logger.debug(\n                    f\"POP API call completed: {method} {endpoint}\",\n                    extra={\n                        \"endpoint\": endpoint,\n                        \"method\": method,\n                        \"duration_ms\": round(duration_ms, 2),\n                        \"request_id\": request_id,\n                        \"success\": True,\n                    },\n                )\n\n                await self._circuit_breaker.record_success()\n\n                return response_data, request_id\n\n            except httpx.TimeoutException:\n                duration_ms = (time.monotonic() - attempt_start) * 1000\n                logger.warning(\n                    \"POP API request timeout\",\n                    extra={\n                        \"endpoint\": endpoint,\n                        \"timeout_seconds\": self._task_timeout,\n                    },\n                )\n                logger.error(\n                    f\"POP API call failed: {method} {endpoint}\",\n                    extra={\n                        \"endpoint\": endpoint,\n                        \"method\": method,\n                        \"duration_ms\": round(duration_ms, 2),\n                        \"status_code\": None,\n                        \"error\": \"Request timed out\",\n                        \"error_type\": \"TimeoutError\",\n                        \"retry_attempt\": attempt,\n                        \"request_id\": request_id,\n                        \"success\": False,\n                    },\n                )\n                await self._circuit_breaker.record_failure()\n\n                if attempt < self._max_retries - 1:\n                    delay = self._retry_delay * (2**attempt)\n                    logger.warning(\n                        f\"POP request attempt {attempt + 1} timed out, \"\n                        f\"retrying in {delay}s\",\n                        extra={\n                            \"attempt\": attempt + 1,\n                            \"max_retries\": self._max_retries,\n                            \"delay_seconds\": delay,\n                        },\n                    )\n                    await asyncio.sleep(delay)\n                    continue\n\n                last_error = POPTimeoutError(\n                    f\"Request timed out after {self._task_timeout}s\",\n                    request_id=request_id,\n                )\n\n            except httpx.RequestError as e:\n                duration_ms = (time.monotonic() - attempt_start) * 1000\n                logger.error(\n                    f\"POP API call failed: {method} {endpoint}\",\n                    extra={\n                        \"endpoint\": endpoint,\n                        \"method\": method,\n                        \"duration_ms\": round(duration_ms, 2),\n                        \"status_code\": None,\n                        \"error\": str(e),\n                        \"error_type\": type(e).__name__,\n                        \"retry_attempt\": attempt,\n                        \"request_id\": request_id,\n                        \"success\": False,\n                    },\n                )\n                await self._circuit_breaker.record_failure()\n\n                if attempt < self._max_retries - 1:\n                    delay = self._retry_delay * (2**attempt)\n                    logger.warning(\n                        f\"POP request attempt {attempt + 1} failed, \"\n                        f\"retrying in {delay}s\",\n                        extra={\n                            \"attempt\": attempt + 1,\n                            \"max_retries\": self._max_retries,\n                            \"delay_seconds\": delay,\n                            \"error\": str(e),\n                        },\n                    )\n                    await asyncio.sleep(delay)\n                    continue\n\n                last_error = POPError(\n                    f\"Request failed: {e}\",\n                    request_id=request_id,\n                )\n\n        if last_error:\n            raise last_error\n        raise POPError(\n            \"Request failed after all retries\",\n            request_id=request_id,\n        )\n\n    async def create_report_task(\n        self,\n        keyword: str,\n        url: str,\n    ) -> POPTaskResult:\n        \"\"\"Create a POP report task for content scoring.\n\n        POSTs to the POP API to create a new content analysis task.\n        The task runs asynchronously and must be polled for results.\n\n        Args:\n            keyword: Target keyword for content optimization\n            url: URL of the page to analyze\n\n        Returns:\n            POPTaskResult with task_id if successful\n        \"\"\"\n        start_time = time.monotonic()\n\n        if not self._available:\n            return POPTaskResult(\n                success=False,\n                error=\"POP not configured (missing API key)\",\n            )\n\n        if not keyword or not url:\n            return POPTaskResult(\n                success=False,\n                error=\"Both keyword and url are required\",\n            )\n\n        logger.info(\n            \"Creating POP report task\",\n            extra={\n                \"keyword\": keyword[:50],\n                \"url\": url[:100],\n            },\n        )\n\n        payload = {\n            \"keyword\": keyword,\n            \"url\": url,\n        }\n\n        try:\n            response_data, request_id = await self._make_request(\n                \"/api/report\",\n                payload,\n                method=\"POST\",\n            )\n\n            # Extract task_id from response\n            task_id = response_data.get(\"task_id\") or response_data.get(\"taskId\")\n\n            if not task_id:\n                # Some APIs return the task_id directly or in a nested structure\n                task_id = response_data.get(\"id\") or response_data.get(\"data\", {}).get(\n                    \"task_id\"\n                )\n\n            duration_ms = (time.monotonic() - start_time) * 1000\n\n            if task_id:\n                logger.info(\n                    \"POP report task created\",\n                    extra={\n                        \"task_id\": task_id,\n                        \"keyword\": keyword[:50],\n                        \"duration_ms\": round(duration_ms, 2),\n                    },\n                )\n                return POPTaskResult(\n                    success=True,\n                    task_id=str(task_id),\n                    status=POPTaskStatus.PENDING,\n                    data=response_data,\n                    duration_ms=duration_ms,\n                    request_id=request_id,\n                )\n            else:\n                logger.warning(\n                    \"POP task created but no task_id returned\",\n                    extra={\n                        \"response_keys\": list(response_data.keys()),\n                    },\n                )\n                return POPTaskResult(\n                    success=False,\n                    error=\"No task_id in response\",\n                    data=response_data,\n                    duration_ms=duration_ms,\n                    request_id=request_id,\n                )\n\n        except POPError as e:\n            duration_ms = (time.monotonic() - start_time) * 1000\n            logger.error(\n                \"Failed to create POP report task\",\n                extra={\n                    \"keyword\": keyword[:50],\n                    \"error\": str(e),\n                    \"duration_ms\": round(duration_ms, 2),\n                },\n            )\n            return POPTaskResult(\n                success=False,\n                error=str(e),\n                duration_ms=duration_ms,\n                request_id=e.request_id,\n            )\n\n    async def get_task_result(\n        self,\n        task_id: str,\n    ) -> POPTaskResult:\n        \"\"\"Get the result of a POP task.\n\n        GETs the task status and results from the POP API.\n\n        Args:\n            task_id: The task ID returned from create_report_task()\n\n        Returns:\n            POPTaskResult with status and data if available\n        \"\"\"\n        start_time = time.monotonic()\n\n        if not self._available:\n            return POPTaskResult(\n                success=False,\n                task_id=task_id,\n                error=\"POP not configured (missing API key)\",\n            )\n\n        if not task_id:\n            return POPTaskResult(\n                success=False,\n                error=\"task_id is required\",\n            )\n\n        logger.debug(\n            \"Getting POP task result\",\n            extra={\"task_id\": task_id},\n        )\n\n        # POP API endpoint for task results\n        # Note: This uses the app.pageoptimizer.pro domain per the notes\n        endpoint = f\"/api/task/{task_id}/results/\"\n\n        try:\n            # Note: GET request still needs apiKey in body for POP\n            response_data, request_id = await self._make_request(\n                endpoint,\n                {},  # Empty payload, apiKey added by _make_request\n                method=\"GET\",\n            )\n\n            duration_ms = (time.monotonic() - start_time) * 1000\n\n            # Parse status from response\n            status_str = (\n                response_data.get(\"status\")\n                or response_data.get(\"task_status\")\n                or \"unknown\"\n            )\n            status_str = status_str.lower()\n\n            if status_str in (\"success\", \"complete\", \"completed\", \"done\"):\n                status = POPTaskStatus.SUCCESS\n            elif status_str in (\"failure\", \"failed\", \"error\"):\n                status = POPTaskStatus.FAILURE\n            elif status_str in (\"pending\", \"queued\", \"waiting\"):\n                status = POPTaskStatus.PENDING\n            elif status_str in (\"processing\", \"running\", \"in_progress\"):\n                status = POPTaskStatus.PROCESSING\n            else:\n                status = POPTaskStatus.UNKNOWN\n\n            logger.debug(\n                \"POP task status retrieved\",\n                extra={\n                    \"task_id\": task_id,\n                    \"status\": status.value,\n                    \"duration_ms\": round(duration_ms, 2),\n                },\n            )\n\n            return POPTaskResult(\n                success=True,\n                task_id=task_id,\n                status=status,\n                data=response_data,\n                duration_ms=duration_ms,\n                request_id=request_id,\n            )\n\n        except POPError as e:\n            duration_ms = (time.monotonic() - start_time) * 1000\n            logger.error(\n                \"Failed to get POP task result\",\n                extra={\n                    \"task_id\": task_id,\n                    \"error\": str(e),\n                    \"duration_ms\": round(duration_ms, 2),\n                },\n            )\n            return POPTaskResult(\n                success=False,\n                task_id=task_id,\n                error=str(e),\n                duration_ms=duration_ms,\n                request_id=e.request_id,\n            )\n\n    async def poll_for_result(\n        self,\n        task_id: str,\n        poll_interval: float | None = None,\n        timeout: float | None = None,\n    ) -> POPTaskResult:\n        \"\"\"Poll for task completion with configurable interval and timeout.\n\n        Continuously polls get_task_result() until the task completes\n        (SUCCESS or FAILURE) or the timeout is reached.\n\n        Args:\n            task_id: The task ID to poll\n            poll_interval: Seconds between polls. Defaults to settings (3s).\n            timeout: Maximum seconds to wait. Defaults to settings (300s).\n\n        Returns:\n            POPTaskResult with final status and data\n\n        Raises:\n            POPTimeoutError: If timeout is reached before task completes\n        \"\"\"\n        poll_interval = poll_interval or self._task_poll_interval\n        timeout = timeout or self._task_timeout\n\n        start_time = time.monotonic()\n        poll_count = 0\n\n        logger.info(\n            \"Starting POP task polling\",\n            extra={\n                \"task_id\": task_id,\n                \"poll_interval_seconds\": poll_interval,\n                \"timeout_seconds\": timeout,\n            },\n        )\n\n        while True:\n            elapsed = time.monotonic() - start_time\n\n            # Check timeout\n            if elapsed >= timeout:\n                logger.error(\n                    \"POP task polling timed out\",\n                    extra={\n                        \"task_id\": task_id,\n                        \"elapsed_seconds\": round(elapsed, 2),\n                        \"timeout_seconds\": timeout,\n                        \"poll_count\": poll_count,\n                    },\n                )\n                raise POPTimeoutError(\n                    f\"Task {task_id} timed out after {elapsed:.1f}s\",\n                    request_id=task_id,\n                )\n\n            # Get current status\n            result = await self.get_task_result(task_id)\n            poll_count += 1\n\n            if not result.success:\n                # API error - return the error result\n                logger.warning(\n                    \"POP task poll failed\",\n                    extra={\n                        \"task_id\": task_id,\n                        \"error\": result.error,\n                        \"poll_count\": poll_count,\n                    },\n                )\n                return result\n\n            # Check for terminal states\n            if result.status == POPTaskStatus.SUCCESS:\n                logger.info(\n                    \"POP task completed successfully\",\n                    extra={\n                        \"task_id\": task_id,\n                        \"elapsed_seconds\": round(elapsed, 2),\n                        \"poll_count\": poll_count,\n                    },\n                )\n                return result\n\n            if result.status == POPTaskStatus.FAILURE:\n                logger.warning(\n                    \"POP task failed\",\n                    extra={\n                        \"task_id\": task_id,\n                        \"elapsed_seconds\": round(elapsed, 2),\n                        \"poll_count\": poll_count,\n                        \"data\": result.data,\n                    },\n                )\n                return result\n\n            # Still processing - wait and poll again\n            logger.debug(\n                \"POP task still processing\",\n                extra={\n                    \"task_id\": task_id,\n                    \"status\": result.status.value,\n                    \"elapsed_seconds\": round(elapsed, 2),\n                    \"poll_count\": poll_count,\n                    \"next_poll_in\": poll_interval,\n                },\n            )\n\n            await asyncio.sleep(poll_interval)\n\n\n# Global POP client instance\n_pop_client: POPClient | None = None\n\n\nasync def init_pop() -> POPClient:\n    \"\"\"Initialize the global POP client.\n\n    Returns:\n        Initialized POPClient instance\n    \"\"\"\n    global _pop_client\n    if _pop_client is None:\n        _pop_client = POPClient()\n        if _pop_client.available:\n            logger.info(\"POP client initialized\")\n        else:\n            logger.info(\"POP not configured (missing API key)\")\n    return _pop_client\n\n\nasync def close_pop() -> None:\n    \"\"\"Close the global POP client.\"\"\"\n    global _pop_client\n    if _pop_client:\n        await _pop_client.close()\n        _pop_client = None\n\n\nasync def get_pop_client() -> POPClient:\n    \"\"\"Dependency for getting POP client.\n\n    Usage:\n        @app.get(\"/content-score\")\n        async def score_content(\n            url: str,\n            client: POPClient = Depends(get_pop_client)\n        ):\n            result = await client.score_content(url)\n            ...\n    \"\"\"\n    global _pop_client\n    if _pop_client is None:\n        await init_pop()\n    return _pop_client  # type: ignore[return-value]\n","numLines":1059,"startLine":1,"totalLines":1059}}}
