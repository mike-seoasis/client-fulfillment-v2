  48→\n    49→\n    50→class CrawlValidationError(CrawlServiceError):\n    51→    \"\"\"Raised when crawl validation fails.\"\"\"\n    52→\n    53→    def __init__(self, field: str, value: Any, message: str):\n    54→        self.field = field\n    55→        self.value = value\n    56→        self.message = message\n    57→        super().__init__(f\"Validation failed for '{field}': {message}\")\n    58→\n    59→\n    60→class CrawlNotFoundError(CrawlServiceError):\n    61→    \"\"\"Raised when a crawl job or page is not found.\"\"\"\n    62→\n    63→    def __init__(self, entity_type: str, entity_id: str):\n    64→        self.entity_type = entity_type\n    65→        self.entity_id = entity_id\n    66→        super().__init__(f\"{entity_type} not found: {entity_id}\")\n    67→\n    68→\n    69→class CrawlPatternError(CrawlServiceError):\n    70→    \"\"\"Raised when pattern matching fails or patterns are invalid.\"\"\"\n    71→\n    72→    def __init__(self, pattern: str, message: str):\n    73→        self.pattern = pattern\n    74→        self.message = message\n    75→        super().__init__(f\"Invalid pattern '{pattern}': {message}\")\n    76→\n    77→\n    78→@dataclass\n    79→class CrawlConfig:\n    80→    \"\"\"Configuration for a crawl operation.\"\"\"\n    81→\n    82→    start_url: str\n    83→    include_patterns: list[str] = field(default_factory=list)\n    84→    exclude_patterns: list[str] = field(default_factory=list)\n    85→    max_pages: int = 100\n    86→    max_depth: int = 3\n    87→    crawl_options: CrawlOptions | None = None\n    88→\n    89→    def to_dict(self) -> dict[str, Any]:\n    90→        \"\"\"Convert config to dictionary for storage.\"\"\"\n    91→        return {\n    92→            \"start_url\": self.start_url,\n    93→            \"include_patterns\": self.include_patterns,\n    94→            \"exclude_patterns\": self.exclude_patterns,\n    95→            \"max_pages\": self.max_pages,\n    96→            \"max_depth\": self.max_depth,\n    97→        }\n    98→\n    99→\n   100→@dataclass\n   101→class CrawlProgress:\n   102→    \"\"\"Progress tracking for a crawl operation.\"\"\"\n   103→\n   104→    pages_crawled: int = 0\n   105→    pages_failed: int = 0\n   106→    pages_skipped: int = 0\n   107→    urls_discovered: int = 0\n   108→    current_depth: int = 0\n   109→    status: str = \"pending\"\n   110→    errors: list[dict[str, Any]] = field(default_factory=list)\n   111→    started_at: datetime | None = None\n   112→    completed_at: datetime | None = None\n   113→\n   114→    def to_dict(self) -> dict[str, Any]:\n   115→        \"\"\"Convert progress to dictionary for storage.\"\"\"\n   116→        return {\n   117→            \"pages_crawled\": self.pages_crawled,\n   118→            \"pages_failed\": self.pages_failed,\n   119→            \"pages_skipped\": self.pages_skipped,\n   120→            \"urls_discovered\": self.urls_discovered,\n   121→            \"current_depth\": self.current_depth,\n   122→        }\n   123→\n   124→\n   125→class PatternMatcher:\n   126→    \"\"\"URL pattern matching for include/exclude filtering.\n   127→\n   128→    Supports glob patterns for flexible URL matching:\n   129→    - `/products/*` - matches /products/item1, /products/item2\n   130→    - `/**/products/*` - matches /any/path/products/item\n   131→    - `*.pdf` - matches any URL ending in .pdf\n   132→    \"\"\"\n   133→\n   134→    def __init__(\n   135→        self,\n   136→        include_patterns: list[str] | None = None,\n   137→        exclude_patterns: list[str] | None = None,\n   138→    ) -> None:\n   139→        \"\"\"Initialize pattern matcher.\n   140→\n   141→        Args:\n   142→            include_patterns: Glob patterns for URLs to include (high priority)\n   143→            exclude_patterns: Glob patterns for URLs to exclude (checked first)\n   144→        \"\"\"\n   145→        logger.debug(\n   146→            \"PatternMatcher.__init__ called\",\n   147→            extra={\n   148→                \"include_pattern_count\": len(include_patterns) if include_patterns else 0,\n   149→                \"exclude_pattern_count\": len(exclude_patterns) if exclude_patterns else 0,\n   150→            },\n   151→        )\n   152→\n   153→        self._include_patterns = include_patterns or []\n   154→        self._exclude_patterns = exclude_patterns or []\n   155→\n   156→        # Validate patterns\n   157→        for pattern in self._include_patterns:\n   158→            self._validate_pattern(pattern, \"include\")\n   159→        for pattern in self._exclude_patterns:\n   160→            self._validate_pattern(pattern, \"exclude\")\n   161→\n   162→    def _validate_pattern(self, pattern: str, pattern_type: str) -> None:\n   163→        \"\"\"Validate a glob pattern.\n   164→\n   165→        Args:\n   166→            pattern: Pattern to validate\n   167→            pattern_type: Type of pattern ('include' or 'exclude')\n   168→\n   169→        Raises:\n   170→            CrawlPatternError: If pattern is invalid\n   171→        \"\"\"\n   172→        if not pattern or not pattern.strip():\n   173→            logger.warning(\n   174→                \"Validation failed: empty pattern\",\n   175→                extra={\"field\": f\"{pattern_type}_patterns\", \"value\": pattern},\n   176→            )\n   177→            raise CrawlPatternError(pattern, \"Pattern cannot be empty\")\n   178→\n   179→        # Check for obviously invalid patterns\n   180→        if pattern.count(\"**\") > 5:\n   181→            logger.warning(\n   182→                \"Validation failed: too many wildcards\",\n   183→                extra={\"field\": f\"{pattern_type}_patterns\", \"value\": pattern},\n   184→            )\n   185→            raise CrawlPatternError(pattern, \"Pattern has too many ** wildcards\")\n   186→\n   187→    @property\n   188→    def include_patterns(self) -> list[str]:\n   189→        \"\"\"Get include patterns.\"\"\"\n   190→        return self._include_patterns.copy()\n   191→\n   192→    @property\n   193→    def exclude_patterns(self) -> list[str]:\n   194→        \"\"\"Get exclude patterns.\"\"\"\n   195→        return self._exclude_patterns.copy()\n   196→\n   197→    def matches_include(self, url: str) -> tuple[bool, str | None]:\n   198→        \"\"\"Check if URL matches any include pattern.\n   199→\n   200→        Args:\n   201→            url: URL to check\n   202→\n   203→        Returns:\n   204→            Tuple of (matches, matched_pattern)\n   205→        \"\"\"\n   206→        logger.debug(\n   207→            \"matches_include() called\",\n   208→            extra={\"url\": url[:200] if url else \"\"},\n   209→        )\n   210→\n   211→        if not self._include_patterns:\n   212→            # No include patterns means all URLs are included\n   213→            return True, None\n   214→\n   215→        try:\n   216→            parsed = urlparse(url)\n   217→            path = parsed.path or \"/\"\n   218→        except Exception as e:\n   219→            logger.warning(\n   220→                \"Failed to parse URL for include matching\",\n   221→                extra={\n   222→                    \"url\": url[:200] if url else \"\",\n   223→                    \"error_type\": type(e).__name__,\n   224→                    \"error_message\": str(e),\n   225→                },\n   226→            )\n   227→            return False, None\n   228→\n   229→        for pattern in self._include_patterns:\n   230→            if fnmatch(path, pattern):\n   231→                logger.debug(\n   232→                    \"URL matched include pattern\",\n   233→                    extra={\"url\": url[:200], \"pattern\": pattern},\n   234→                )\n   235→                return True, pattern\n   236→\n   237→        logger.debug(\n   238→            \"URL did not match any include pattern\",\n   239→            extra={\"url\": url[:200], \"pattern_count\": len(self._include_patterns)},\n   240→        )\n   241→        return False, None\n   242→\n   243→    def matches_exclude(self, url: str) -> tuple[bool, str | None]:\n   244→        \"\"\"Check if URL matches any exclude pattern.\n   245→\n   246→        Args:\n   247→            url: URL to check\n   248→\n   249→        Returns:\n   250→            Tuple of (matches, matched_pattern)\n   251→        \"\"\"\n   252→        logger.debug(\n   253→            \"matches_exclude() called\",\n   254→            extra={\"url\": url[:200] if url else \"\"},\n   255→        )\n   256→\n   257→        if not self._exclude_patterns:\n   258→            return False, None\n   259→\n   260→        try:\n   261→            parsed = urlparse(url)\n   262→            path = parsed.path or \"/\"\n   263→        except Exception as e:\n   264→            logger.warning(\n   265→                \"Failed to parse URL for exclude matching\",\n   266→                extra={\n   267→                    \"url\": url[:200] if url else \"\",\n   268→                    \"error_type\": type(e).__name__,\n   269→                    \"error_message\": str(e),\n   270→                },\n   271→            )\n   272→            return False, None\n   273→\n   274→        for pattern in self._exclude_patterns:\n   275→            if fnmatch(path, pattern):\n   276→                logger.debug(\n   277→                    \"URL matched exclude pattern\",\n   278→                    extra={\"url\": url[:200], \"pattern\": pattern},\n   279→                )\n   280→                return True, pattern\n   281→\n   282→        return False, None\n   283→\n   284→    def should_crawl(self, url: str) -> tuple[bool, str]:\n   285→        \"\"\"Determine if URL should be crawled based on patterns.\n   286→\n   287→        Exclusion patterns are checked first, then inclusion patterns.\n   288→\n   289→        Args:\n   290→            url: URL to check\n   291→\n   292→        Returns:\n   293→            Tuple of (should_crawl, reason)\n   294→        \"\"\"\n   295→        logger.debug(\n   296→            \"should_crawl() called\",\n   297→            extra={\"url\": url[:200] if url else \"\"},\n   298→        )\n   299→\n   300→        # Check exclusions first\n   301→        is_excluded, exclude_pattern = self.matches_exclude(url)\n   302→        if is_excluded:\n   303→            reason = f\"Excluded by pattern: {exclude_pattern}\"\n   304→            logger.debug(\n   305→                \"URL should not be crawled\",\n   306→                extra={\"url\": url[:200], \"reason\": reason},\n   307→            )\n   308→            return False, reason\n   309→\n   310→        # Check inclusions\n   311→        is_included, include_pattern = self.matches_include(url)\n   312→        if is_included:\n   313→            if include_pattern:\n   314→                reason = f\"Included by pattern: {include_pattern}\"\n   315→            else:\n   316→                reason = \"Included (no patterns specified)\"\n   317→            logger.debug(\n   318→                \"URL should be crawled\",\n   319→                extra={\"url\": url[:200], \"reason\": reason},\n   320→            )\n   321→            return True, reason\n   322→\n   323→        reason = \"Not matched by any include pattern\"\n   324→        logger.debug(\n   325→            \"URL should not be crawled\",\n   326→            extra={\"url\": url[:200], \"reason\": reason},\n   327→        )\n   328→        return False, reason\n   329→\n   330→    def determine_priority(self, url: str) -> URLPriority:\n   331→        \"\"\"Determine crawl priority for URL based on patterns.\n   332→\n   333→        Args:\n   334→            url: URL to determine priority for\n   335→\n   336→        Returns:\n   337→            URLPriority value\n   338→        \"\"\"\n   339→        is_included, _ = self.matches_include(url)\n   340→        if is_included and self._include_patterns:\n   341→            return URLPriority.INCLUDE\n   342→        return URLPriority.OTHER\n   343→\n   344→\n   345→class CrawlService:\n   346→    \"\"\"Service for orchestrating web crawling operations.\n   347→\n   348→    Coordinates between:\n   349→    - URLPriorityQueue for URL management and prioritization\n   350→    - PatternMatcher for include/exclude filtering\n   351→    - Crawl4AIClient for actual crawling\n   352→    - Database for persistence\n   353→\n   354→    Example usage:\n   355→        async with get_session() as session:\n   356→            service = CrawlService(session, crawl4ai_client)\n   357→\n   358→            # Start a crawl\n   359→            history = await service.start_crawl(\n   360→                project_id=\"...\",\n   361→                config=CrawlConfig(\n   362→                    start_url=\"https://example.com\",\n   363→                    include_patterns=[\"/products/*\", \"/services/*\"],\n   364→                    exclude_patterns=[\"/admin/*\", \"/api/*\"],\n   365→                    max_pages=50,\n   366→                ),\n   367→            )\n   368→\n   369→            # Check progress\n   370→            progress = await service.get_crawl_progress(history.id)\n   371→    \"\"\"\n   372→\n   373→    def __init__(\n   374→        self,\n   375→        session: AsyncSession,\n   376→        crawl4ai_client: Crawl4AIClient | None = None,\n   377→    ) -> None:\n   378→        \"\"\"Initialize CrawlService.\n   379→\n   380→        Args:\n   381→            session: Async SQLAlchemy session\n   382→            crawl4ai_client: Optional Crawl4AI client (uses global if not provided)\n   383→        \"\"\"\n   384→        self.session = session\n   385→        self._crawl4ai = crawl4ai_client\n   386→        logger.debug(\"CrawlService initialized\")\n   387→\n   388→    async def _get_crawl4ai(self) -> Crawl4AIClient:\n   389→        \"\"\"Get Crawl4AI client (lazy load if needed).\"\"\"\n   390→        if self._crawl4ai is None:\n   391→            from app.integrations.crawl4ai import get_crawl4ai\n   392→\n   393→            self._crawl4ai = await get_crawl4ai()\n   394→        return self._crawl4ai\n   395→\n   396→    # -------------------------------------------------------------------------\n   397→    # Pattern Matching Methods\n   398→    # -------------------------------------------------------------------------\n   399→\n   400→    def create_pattern_matcher(\n   401→        self,\n   402→        include_patterns: list[str] | None = None,\n   403→        exclude_patterns: list[str] | None = None,\n   404→    ) -> PatternMatcher:\n   405→        \"\"\"Create a pattern matcher with validation.\n   406→\n   407→        Args:\n   408→            include_patterns: Glob patterns for URLs to include\n   409→            exclude_patterns: Glob patterns for URLs to exclude\n   410→\n   411→        Returns:\n   412→            Configured PatternMatcher\n   413→\n   414→        Raises:\n   415→            CrawlPatternError: If patterns are invalid\n   416→        \"\"\"\n   417→        start_time = time.monotonic()\n   418→        logger.debug(\n   419→            \"Creating pattern matcher\",\n   420→            extra={\n   421→                \"include_patterns\": include_patterns[:5] if include_patterns else [],\n   422→                \"exclude_patterns\": exclude_patterns[:5] if exclude_patterns else [],\n   423→            },\n   424→        )\n   425→\n   426→        try:\n   427→            matcher = PatternMatcher(\n   428→                include_patterns=include_patterns,\n   429→                exclude_patterns=exclude_patterns,\n   430→            )\n   431→\n   432→            duration_ms = (time.monotonic() - start_time) * 1000\n   433→            logger.debug(\n   434→                \"Pattern matcher created\",\n   435→                extra={\"duration_ms\": round(duration_ms, 2)},\n   436→            )\n   437→\n   438→            return matcher\n   439→\n   440→        except CrawlPatternError:\n   441→            raise\n   442→        except Exception as e:\n   443→            logger.error(\n   444→                \"Failed to create pattern matcher\",\n   445→                extra={\n   446→                    \"error_type\": type(e).__name__,\n   447→                    \"error_message\": str(e),\n   448→                },\n   449→                exc_info=True,\n   450→            )\n   451→            raise\n   452→\n   453→    def filter_urls(\n   454→        self,\n   455→        urls: list[str],\n   456→        include_patterns: list[str] | None = None,\n   457→        exclude_patterns: list[str] | None = None,\n   458→    ) -> list[str]:\n   459→        \"\"\"Filter URLs using include/exclude patterns.\n   460→\n   461→        Args:\n   462→            urls: List of URLs to filter\n   463→            include_patterns: Glob patterns for URLs to include\n   464→            exclude_patterns: Glob patterns for URLs to exclude\n   465→\n   466→        Returns:\n   467→            Filtered list of URLs that should be crawled\n   468→        \"\"\"\n   469→        start_time = time.monotonic()\n   470→        logger.debug(\n   471→            \"filter_urls() called\",\n   472→            extra={\n   473→                \"url_count\": len(urls),\n   474→                \"include_pattern_count\": len(include_patterns) if include_patterns else 0,\n   475→                \"exclude_pattern_count\": len(exclude_patterns) if exclude_patterns else 0,\n   476→            },\n   477→        )\n   478→\n   479→        try:\n   480→            matcher = self.create_pattern_matcher(\n   481→                include_patterns=include_patterns,\n   482→                exclude_patterns=exclude_patterns,\n   483→            )\n   484→\n   485→            filtered: list[str] = []\n   486→            excluded_count = 0\n   487→            not_included_count = 0\n   488→\n   489→            for url in urls:\n   490→                should_crawl, reason = matcher.should_crawl(url)\n   491→                if should_crawl:\n   492→                    filtered.append(url)\n   493→                elif \"Excluded\" in reason:\n   494→                    excluded_count += 1\n   495→                else:\n   496→                    not_included_count += 1\n   497→\n   498→            duration_ms = (time.monotonic() - start_time) * 1000\n   499→            logger.debug(\n   500→                \"URLs filtered\",\n   501→                extra={\n   502→                    \"input_count\": len(urls),\n   503→                    \"output_count\": len(filtered),\n   504→                    \"excluded_count\": excluded_count,\n   505→                    \"not_included_count\": not_included_count,\n   506→                    \"duration_ms\": round(duration_ms, 2),\n   507→                },\n   508→            )\n   509→\n   510→            if duration_ms > SLOW_OPERATION_THRESHOLD_MS:\n   511→                logger.warning(\n   512→                    \"Slow URL filtering\",\n   513→                    extra={\n   514→                        \"url_count\": len(urls),\n   515→                        \"duration_ms\": round(duration_ms, 2),\n   516→                    },\n   517→                )\n   518→\n   519→            return filtered\n   520→\n   521→        except CrawlServiceError:\n   522→            raise\n   523→        except Exception as e:\n   524→            logger.error(\n   525→                \"Failed to filter URLs\",\n   526→                extra={\n   527→                    \"url_count\": len(urls),\n   528→                    \"error_type\": type(e).__name__,\n   529→                    \"error_message\": str(e),\n   530→                },\n   531→                exc_info=True,\n   532→            )\n   533→            raise\n   534→\n   535→    # -------------------------------------------------------------------------\n   536→    # Crawl Job Management\n   537→    # -----------------------------------------------------------