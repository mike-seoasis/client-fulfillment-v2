\n   704→\n   705→        if not isinstance(searches_data, list):\n   706→            return related_searches\n   707→\n   708→        for item in searches_data:\n   709→            if not isinstance(item, dict):\n   710→                continue\n   711→\n   712→            query = item.get(\"query\")\n   713→            if not query:\n   714→                continue\n   715→\n   716→            related_searches.append(\n   717→                {\n   718→                    \"query\": str(query),\n   719→                    \"relevance\": None,  # POP doesn't provide relevance score\n   720→                }\n   721→            )\n   722→\n   723→        return related_searches\n   724→\n   725→    def _extract_competitors(self, raw_data: dict[str, Any]) -> list[dict[str, Any]]:\n   726→        \"\"\"Extract competitor data from competitors array.\n   727→\n   728→        POP API provides competitors with:\n   729→        - url: competitor page URL\n   730→        - title: page title\n   731→        - pageScore: optimization score\n   732→        - plus additional data like h2Texts, h3Texts, schemaTypes, etc.\n   733→        \"\"\"\n   734→        competitors: list[dict[str, Any]] = []\n   735→        competitors_data = raw_data.get(\"competitors\", [])\n   736→\n   737→        if not isinstance(competitors_data, list):\n   738→            return competitors\n   739→\n   740→        for idx, item in enumerate(competitors_data):\n   741→            if not isinstance(item, dict):\n   742→                continue\n   743→\n   744→            url = item.get(\"url\")\n   745→            if not url:\n   746→                continue\n   747→\n   748→            page_score = item.get(\"pageScore\")\n   749→\n   750→            competitors.append(\n   751→                {\n   752→                    \"url\": str(url),\n   753→                    \"title\": str(item.get(\"title\", \"\")) if item.get(\"title\") else None,\n   754→                    \"page_score\": float(page_score)\n   755→                    if isinstance(page_score, (int, float))\n   756→                    else None,\n   757→                    \"word_count\": None,  # POP doesn't provide word count directly per competitor\n   758→                    \"position\": idx + 1,  # Infer position from array order\n   759→                }\n   760→            )\n   761→\n   762→        return competitors\n   763→\n   764→    def _extract_page_score_target(self, raw_data: dict[str, Any]) -> float | None:\n   765→        \"\"\"Extract page score target from POP response.\n   766→\n   767→        POP API provides pageScore or pageScoreValue at top level\n   768→        of cleanedContentBrief.\n   769→        \"\"\"\n   770→        # Try cleanedContentBrief first\n   771→        content_brief = raw_data.get(\"cleanedContentBrief\", {})\n   772→        if isinstance(content_brief, dict):\n   773→            page_score = content_brief.get(\"pageScore\")\n   774→            if isinstance(page_score, (int, float)):\n   775→                return float(page_score)\n   776→\n   777→            page_score_value = content_brief.get(\"pageScoreValue\")\n   778→            if page_score_value is not None:\n   779→                try:\n   780→                    return float(page_score_value)\n   781→                except (ValueError, TypeError):\n   782→                    pass\n   783→\n   784→        # Try top-level\n   785→        page_score = raw_data.get(\"pageScore\")\n   786→        if isinstance(page_score, (int, float)):\n   787→            return float(page_score)\n   788→\n   789→        return None\n   790→\n   791→    async def fetch_brief(\n   792→        self,\n   793→        project_id: str,\n   794→        page_id: str,\n   795→        keyword: str,\n   796→        target_url: str,\n   797→    ) -> POPContentBriefResult:\n   798→        \"\"\"Fetch a content brief from POP API for a keyword/URL.\n   799→\n   800→        Creates a POP report task, polls for completion, and parses\n   801→        the results into a structured content brief.\n   802→\n   803→        Args:\n   804→            project_id: Project ID for logging context\n   805→            page_id: Page ID for logging context\n   806→            keyword: Target keyword for content optimization\n   807→            target_url: URL of the page to optimize\n   808→\n   809→        Returns:\n   810→            POPContentBriefResult with brief data or error\n   811→        \"\"\"\n   812→        start_time = time.monotonic()\n   813→\n   814→        # Method entry log with sanitized parameters\n   815→        logger.debug(\n   816→            \"fetch_brief method entry\",\n   817→            extra={\n   818→                \"project_id\": project_id,\n   819→                \"page_id\": page_id,\n   820→                \"keyword\": keyword[:50] if keyword else \"\",\n   821→                \"target_url\": target_url[:100] if target_url else \"\",\n   822→            },\n   823→        )\n   824→\n   825→        # Validate inputs\n   826→        if not keyword or not keyword.strip():\n   827→            logger.warning(\n   828→                \"Content brief validation failed - empty keyword\",\n   829→                extra={\n   830→                    \"field\": \"keyword\",\n   831→                    \"value\": \"\",\n   832→                    \"project_id\": project_id,\n   833→                    \"page_id\": page_id,\n   834→                },\n   835→            )\n   836→            raise POPContentBriefValidationError(\n   837→                \"keyword\",\n   838→                \"\",\n   839→                \"Keyword cannot be empty\",\n   840→                project_id=project_id,\n   841→                page_id=page_id,\n   842→            )\n   843→\n   844→        if not target_url or not target_url.strip():\n   845→            logger.warning(\n   846→                \"Content brief validation failed - empty target_url\",\n   847→                extra={\n   848→                    \"field\": \"target_url\",\n   849→                    \"value\": \"\",\n   850→                    \"project_id\": project_id,\n   851→                    \"page_id\": page_id,\n   852→                },\n   853→            )\n   854→            raise POPContentBriefValidationError(\n   855→                \"target_url\",\n   856→                \"\",\n   857→                \"Target URL cannot be empty\",\n   858→                project_id=project_id,\n   859→                page_id=page_id,\n   860→            )\n   861→\n   862→        keyword = keyword.strip()\n   863→        target_url = target_url.strip()\n   864→\n   865→        try:\n   866→            client = await self._get_client()\n   867→\n   868→            # Phase transition: brief_fetch_started\n   869→            logger.info(\n   870→                \"brief_fetch_started\",\n   871→                extra={\n   872→                    \"project_id\": project_id,\n   873→                    \"page_id\": page_id,\n   874→                    \"keyword\": keyword[:50],\n   875→                    \"target_url\": target_url[:100],\n   876→                },\n   877→            )\n   878→\n   879→            # Step 1: Create report task\n   880→            logger.info(\n   881→                \"Creating POP report task for content brief\",\n   882→                extra={\n   883→                    \"project_id\": project_id,\n   884→                    \"page_id\": page_id,\n   885→                    \"keyword\": keyword[:50],\n   886→                },\n   887→            )\n   888→\n   889→            task_result = await client.create_report_task(\n   890→                keyword=keyword,\n   891→                url=target_url,\n   892→            )\n   893→\n   894→            if not task_result.success or not task_result.task_id:\n   895→                duration_ms = (time.monotonic() - start_time) * 1000\n   896→                logger.error(\n   897→                    \"Failed to create POP report task\",\n   898→                    extra={\n   899→                        \"project_id\": project_id,\n   900→                        \"page_id\": page_id,\n   901→                        \"keyword\": keyword[:50],\n   902→                        \"error\": task_result.error,\n   903→                        \"duration_ms\": round(duration_ms, 2),\n   904→                    },\n   905→                )\n   906→                # Method exit log on task creation failure\n   907→                logger.debug(\n   908→                    \"fetch_brief method exit\",\n   909→                    extra={\n   910→                        \"project_id\": project_id,\n   911→                        \"page_id\": page_id,\n   912→                        \"success\": False,\n   913→                        \"brief_id\": None,\n   914→                        \"error\": task_result.error,\n   915→                        \"duration_ms\": round(duration_ms, 2),\n   916→                    },\n   917→                )\n   918→                return POPContentBriefResult(\n   919→                    success=False,\n   920→                    keyword=keyword,\n   921→                    target_url=target_url,\n   922→                    error=task_result.error or \"Failed to create report task\",\n   923→                    duration_ms=duration_ms,\n   924→                    request_id=task_result.request_id,\n   925→                )\n   926→\n   927→            task_id = task_result.task_id\n   928→\n   929→            logger.info(\n   930→                \"POP report task created, polling for results\",\n   931→                extra={\n   932→                    \"project_id\": project_id,\n   933→                    \"page_id\": page_id,\n   934→                    \"keyword\": keyword[:50],\n   935→                    \"task_id\": task_id,\n   936→                },\n   937→            )\n   938→\n   939→            # Step 2: Poll for task completion\n   940→            poll_result = await client.poll_for_result(task_id)\n   941→\n   942→            if not poll_result.success:\n   943→                duration_ms = (time.monotonic() - start_time) * 1000\n   944→                logger.error(\n   945→                    \"POP task polling failed\",\n   946→                    extra={\n   947→                        \"project_id\": project_id,\n   948→                        \"page_id\": page_id,\n   949→                        \"keyword\": keyword[:50],\n   950→                        \"task_id\": task_id,\n   951→                        \"error\": poll_result.error,\n   952→                        \"duration_ms\": round(duration_ms, 2),\n   953→                    },\n   954→                )\n   955→                # Method exit log on polling failure\n   956→                logger.debug(\n   957→                    \"fetch_brief method exit\",\n   958→                    extra={\n   959→                        \"project_id\": project_id,\n   960→                        \"page_id\": page_id,\n   961→                        \"task_id\": task_id,\n   962→                        \"success\": False,\n   963→                        \"brief_id\": None,\n   964→                        \"error\": poll_result.error,\n   965→                        \"duration_ms\": round(duration_ms, 2),\n   966→                    },\n   967→                )\n   968→                return POPContentBriefResult(\n   969→                    success=False,\n   970→                    keyword=keyword,\n   971→                    target_url=target_url,\n   972→                    task_id=task_id,\n   973→                    error=poll_result.error or \"Task polling failed\",\n   974→                    duration_ms=duration_ms,\n   975→                    request_id=poll_result.request_id,\n   976→                )\n   977→\n   978→            if poll_result.status == POPTaskStatus.FAILURE:\n   979→                duration_ms = (time.monotonic() - star