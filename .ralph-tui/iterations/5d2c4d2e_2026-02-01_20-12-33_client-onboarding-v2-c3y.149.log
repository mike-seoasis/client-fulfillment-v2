y logs\n    46→SENSITIVE_FIELDS = {\n    47→    \"password\",\n    48→    \"token\",\n    49→    \"secret\",\n    50→    \"api_key\",\n    51→    \"authorization\",\n    52→    \"credit_card\",\n    53→}\n    54→\n    55→\n    56→def sanitize_body(body: dict[str, Any]) -> dict[str, Any]:\n    57→    \"\"\"Redact sensitive fields from request body for logging.\"\"\"\n    58→    if not isinstance(body, dict):\n    59→        return body\n    60→    sanitized: dict[str, Any] = {}\n    61→    for key, value in body.items():\n    62→        if key.lower() in SENSITIVE_FIELDS:\n    63→            sanitized[key] = \"****\"\n    64→        elif isinstance(value, dict):\n    65→            sanitized[key] = sanitize_body(value)\n    66→        else:\n    67→            sanitized[key] = value\n    68→    return sanitized\n    69→\n    70→\n    71→class RequestLoggingMiddleware(BaseHTTPMiddleware):\n    72→    \"\"\"Middleware to log all incoming requests with timing and request_id.\"\"\"\n    73→\n    74→    async def dispatch(self, request: Request, call_next: Any) -> Any:\n    75→        # Generate unique request ID\n    76→        request_id = str(uuid.uuid4())\n    77→        request.state.request_id = request_id\n    78→\n    79→        start_time = time.monotonic()\n    80→        method = request.method\n    81→        path = request.url.path\n    82→\n    83→        # Log incoming request\n    84→        logger.info(\n    85→            \"Request started\",\n    86→            extra={\n    87→                \"request_id\": request_id,\n    88→                \"method\": method,\n    89→                \"path\": path,\n    90→                \"query_params\": str(request.query_params)\n    91→                if request.query_params\n    92→                else None,\n    93→            },\n    94→        )\n    95→\n    96→        # Log request body at DEBUG level (for non-GET requests)\n    97→        if method not in (\"GET\", \"HEAD\", \"OPTIONS\") and logger.isEnabledFor(\n    98→            logging.DEBUG\n    99→        ):\n   100→            try:\n   101→                body = await request.body()\n   102→                if body:\n   103→                    import json\n   104→\n   105→                    try:\n   106→                        body_json = json.loads(body)\n   107→                        sanitized = sanitize_body(body_json)\n   108→                        logger.debug(\n   109→                            \"Request body\",\n   110→                            extra={\"request_id\": request_id, \"body\": sanitized},\n   111→                        )\n   112→                    except json.JSONDecodeError:\n   113→                        logger.debug(\n   114→                            \"Request body (non-JSON)\",\n   115→                            extra={\"request_id\": request_id, \"body_length\": len(body)},\n   116→                        )\n   117→            except Exception:\n   118→                pass  # Don't fail on body logging errors\n   119→\n   120→        # Process request\n   121→        response = await call_next(request)\n   122→\n   123→        # Calculate timing\n   124→        duration_ms = (time.monotonic() - start_time) * 1000\n   125→        status_code = response.status_code\n   126→\n   127→        # Add request_id to response headers\n   128→        response.headers[\"X-Request-ID\"] = request_id\n   129→\n   130→        # Determine log level based on status code\n   131→        log_extra = {\n   132→            \"request_id\": request_id,\n   133→            \"method\": method,\n   134→            \"path\": path,\n   135→            \"status_code\": status_code,\n   136→            \"duration_ms\": round(duration_ms, 2),\n   137→        }\n   138→\n   139→        if status_code >= 500:\n   140→            logger.error(\"Request failed\", extra=log_extra)\n   141→        elif status_code >= 400:\n   142→            logger.warning(\"Request error\", extra=log_extra)\n   143→        else:\n   144→            logger.info(\"Request completed\", extra=log_extra)\n   145→\n   146→        return response\n   147→\n   148→\n   149→def log_rate_limit_hit(\n   150→    request_id: str, path: str, client_ip: str | None = None\n   151→) -> None:\n   152→    \"\"\"Log rate limit hits at WARNING level.\n   153→\n   154→    Call this from rate limiting middleware/dependencies when a limit is hit.\n   155→    \"\"\"\n   156→    logger.warning(\n   157→        \"Rate limit exceeded\",\n   158→        extra={\n   159→            \"request_id\": request_id,\n   160→            \"path\": path,\n   161→            \"client_ip\": client_ip,\n   162→        },\n   163→    )\n   164→\n   165→\n   166→@asynccontextmanager\n   167→async def lifespan(app: FastAPI) -> Any:\n   168→    \"\"\"Application lifespan manager for startup/shutdown.\n   169→\n   170→    Handles:\n   171→    - Database initialization\n   172→    - Graceful shutdown on SIGTERM\n   173→    \"\"\"\n   174→    settings = get_settings()\n   175→    logger.info(\n   176→        \"Starting application\",\n   177→        extra={\n   178→            \"app_name\": settings.app_name,\n   179→            \"version\": settings.app_version,\n   180→            \"environment\": settings.environment,\n   181→        },\n   182→    )\n   183→\n   184→    # Initialize database\n   185→    try:\n   186→        db_manager.init_db()\n   187→        logger.info(\"Database initialized\")\n   188→    except Exception as e:\n   189→        logger.error(f\"Failed to initialize database: {e}\")\n   190→        raise\n   191→\n   192→    # Initialize Redis (optional - app works without it)\n   193→    redis_available = await redis_manager.init_redis()\n   194→    if redis_available:\n   195→        logger.info(\"Redis initialized\")\n   196→    else:\n   197→        logger.info(\"Redis not available, caching disabled\")\n   198→\n   199→    # Start WebSocket heartbeat task\n   200→    await connection_manager.start_heartbeat()\