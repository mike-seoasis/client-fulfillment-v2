→        self._available = False\n   276→        logger.info(\"Redis connections closed\")\n   277→\n   278→    async def execute(self, operation: str, *args: Any, **kwargs: Any) -> Any | None:\n   279→        \"\"\"Execute a Redis operation with circuit breaker protection.\n   280→\n   281→        Returns None if Redis is unavailable (graceful degradation).\n   282→        \"\"\"\n   283→        if not self._client or not self._circuit_breaker:\n   284→            redis_logger.graceful_fallback(operation, \"Redis not initialized\")\n   285→            return None\n   286→\n   287→        if not await self._circuit_breaker.can_execute():\n   288→            redis_logger.graceful_fallback(operation, \"Circuit breaker open\")\n   289→            return None\n   290→\n   291→        start_time = time.monotonic()\n   292→        key = str(args[0]) if args else \"\"\n   293→\n   294→        try:\n   295→            method = getattr(self._client, operation)\n   296→            result = await method(*args, **kwargs)\n   297→\n   298→            duration_ms = (time.monotonic() - start_time) * 1000\n   299→            redis_logger.operation(operation, key, duration_ms, success=True)\n   300→            await self._circuit_breaker.record_success()\n   301→            return result\n   302→\n   303→        except RedisTimeoutError:\n   304→            duration_ms = (time.monotonic() - start_time) * 1000\n   305→            settings = get_settings()\n   306→            redis_logger.timeout(operation, key, settings.redis_socket_timeout)\n   307→            redis_logger.operation(operation, key, duration_ms, success=False)\n   308→            await self._circuit_breaker.record_failure()\n   309→            return None\n   310→\n   311→        except AuthenticationError as e:\n   312→            duration_ms = (time.monotonic() - start_time) * 1000\n   313→            settings = get_settings()\n   314→            redis_logger.connection_error(e, str(settings.redis_url))\n   315→   