       extra={\n  1058→                        \"project_id\": project_id,\n  1059→                        \"page_id\": page_id,\n  1060→                        \"task_id\": task_id,\n  1061→                        \"keyword\": keyword[:50],\n  1062→                        \"duration_ms\": round(duration_ms, 2),\n  1063→                        \"threshold_ms\": SLOW_OPERATION_THRESHOLD_MS,\n  1064→                    },\n  1065→                )\n  1066→\n  1067→            result = POPContentBriefResult(\n  1068→                success=True,\n  1069→                keyword=keyword,\n  1070→                target_url=target_url,\n  1071→                task_id=task_id,\n  1072→                word_count_target=parsed.get(\"word_count_target\"),\n  1073→                word_count_min=parsed.get(\"word_count_min\"),\n  1074→                word_count_max=parsed.get(\"word_count_max\"),\n  1075→                heading_targets=parsed.get(\"heading_targets\", []),\n  1076→                keyword_targets=parsed.get(\"keyword_targets\", []),\n  1077→                lsi_terms=parsed.get(\"lsi_terms\", []),\n  1078→                entities=parsed.get(\"entities\", []),\n  1079→                related_questions=parsed.get(\"related_questions\", []),\n  1080→                related_searches=parsed.get(\"related_searches\", []),\n  1081→                competitors=parsed.get(\"competitors\", []),\n  1082→                page_score_target=parsed.get(\"page_score_target\"),\n  1083→                raw_response=raw_data,\n  1084→                duration_ms=duration_ms,\n  1085→                request_id=poll_result.request_id,\n  1086→            )\n  1087→\n  1088→            # Method exit log with result summary\n  1089→            logger.debug(\n  1090→                \"fetch_brief method exit\",\n  1091→                extra={\n  1092→                    \"project_id\": project_id,\n  1093→                    \"page_id\": page_id,\n  1094→                    \"task_id\": task_