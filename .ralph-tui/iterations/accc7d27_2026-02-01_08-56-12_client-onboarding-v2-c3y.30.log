→\n   338→        # Close the WebSocket (suppress errors if already closed)\n   339→        with contextlib.suppress(Exception):\n   340→            await conn.websocket.close(code=code, reason=reason)\n   341→\n   342→        conn.state = ConnectionState.CLOSED\n   343→        del self._connections[connection_id]\n   344→        ws_logger.connection_closed(connection_id, reason, code)\n   345→\n   346→    def _subscribe_connection(self, connection_id: str, project_id: str) -> None:\n   347→        \"\"\"Subscribe a connection to a project's updates.\"\"\"\n   348→        conn = self._connections.get(connection_id)\n   349→        if not conn:\n   350→            return\n   351→\n   352→        conn.subscribe(project_id)\n   353→\n   354→        if project_id not in self._project_connections:\n   355→            self._project_connections[project_id] = set()\n   356→        self._project_connections[project_id].add(connection_id)\n   357→\n   358→        ws_logger.subscription_changed(connection_id, project_id, \"subscribed\")\n   359→\n   360→    def _unsubscribe_connection(self, connection_id: str, project_id: str) -> None:\n   361→        \"\"\"Unsubscribe a connection from a project's updates.\"\"\"\n   362→        conn = self._connections.get(connection_id)\n   363→        if conn:\n   364→            conn.unsubscribe(project_id)\n   365→\n   366→        if project_id in self._project_connections:\n   367→            self._project_connections[project_id].discard(connection_id)\n   368→            if not self._project_connections[project_id]:\n   369→                del self._project_connections[project_id]\n   370→\n   371→        ws_logger.subscription_changed(connection_id, project_id, \"unsubscribed\")\n   372→\n   373→    async def _send_message(\n   374→        self, conn: WebSocketConnection, message: dict[str, Any]\n   375→    ) -> bool:\n   376→        \"\"\"Send a message to a specific connection.\n   377→\n   378→        Args:\n   379→            conn: