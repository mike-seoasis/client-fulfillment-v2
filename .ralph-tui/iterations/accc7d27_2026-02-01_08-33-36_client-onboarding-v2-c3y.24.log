   settings = get_settings()\n   186→\n   187→        if not settings.redis_url:\n   188→            logger.info(\"Redis URL not configured, Redis features disabled\")\n   189→            self._available = False\n   190→            return False\n   191→\n   192→        redis_url = str(settings.redis_url)\n   193→\n   194→        # Initialize circuit breaker\n   195→        self._circuit_breaker = CircuitBreaker(\n   196→            CircuitBreakerConfig(\n   197→                failure_threshold=settings.redis_circuit_failure_threshold,\n   198→                recovery_timeout=settings.redis_circuit_recovery_timeout,\n   199→            )\n   200→        )\n   201→\n   202→        # Determine if SSL should be used (Railway Redis uses SSL)\n   203→        use_ssl = redis_url.startswith(\"rediss://\")\n   204→\n   205→        try:\n   206→            # Create connection pool\n   207→            self._pool = ConnectionPool.from_url(\n   208→                redis_url,\n   209→                max_connections=settings.redis_pool_size,\n   210→                socket_connect_timeout=settings.redis_connect_timeout,\n   211→                socket_timeout=settings.redis_socket_timeout,\n   212→                retry_on_timeout=settings.redis_retry_on_timeout,\n   213→                health_check_interval=settings.redis_health_check_interval,\n   214→                # SSL settings for production\n   215→                ssl_cert_reqs=\"required\" if use_ssl else None,\n   216→            )\n   217→\n   218→            # Create client with pool\n   219→            self._client = Redis(connection_pool=self._pool)\n   220→\n   221→            # Test connection with retry logic for cold starts\n   222→            await self._connect_with_retry(redis_url)\n   223→\n   224→            self._available = True\n   225→            redis_logger.connection_success()\n   226→            return True\n   227→\n   228→        except Exception as e:\n   229→            redis_logger.connection_error(e, redis_url)\n   230→            self._available = False\n   231→            # Don't raise - Redis is optional\n   232→            return False\n   233→\n   234→    async def _connect_with_retry(\n   235→        self,\n   236→        redis_url: str,  # noqa: ARG002\n   237→        max_retries: int = 3,\n   238→        base_delay: float = 1.0,\n   239→    ) -> None:\n   240→        \"\"\"Connect to Redis with retry logic for Railway cold starts.\"\"\"\n   241→        last_error: Exception | None = None\n   242→\n   243→        for attempt in range(max_retries):\n   244→            try:\n   245→                if self._client:\n   246→                    await self._client.ping()\n   247→                    return\n   248→            except (RedisConnectionError, RedisTimeoutError) as e:\n   249→                last_error = e\n   250→                if attempt < max_retries - 1:\n   251→                    delay = base_delay * (2**attempt)  # Exponential backoff\n   252→                    logger.warning(\n   253→                        f\"Redis connection attempt {attempt + 1} failed, \"\n   254→                        f\"retrying in {delay}s\",\n   255→                        extra={\n   256→                            \"attempt\": attempt + 1,\n   257→                            \"max_retries\": max_retries,\n   258→                            \"delay_seconds\": delay,\n   259→                            \"error\": str(e),\n   260→                        },\n   261→                    )\n   262→                    await asyncio.sleep(delay)\n   263→\n   264→        if last_error:\n   265→            raise last_error\n   266→\n   267→    async def close(self) -> None:\n   268→        \"\"\"Close Redis connections.\"\"\"\n   269→        if self._client:\n   270→            await self._client.aclose()\n   271→            self._client = None\n   272→        if self._pool:\n   273→            await self._pool.disconnect()\n   274→            self._pool = None\n   275→        self._available = False\n   276→        logger.info(\"Redis connections closed\")\n   277→\n   278→    async def execute(self, operation: str, *args: Any, **kwargs: Any) -> Any | None:\n   279→        \"\"\"Execute a Redis operation with circuit breaker protection.\n   280→\n   281→        Returns None if Redis is unavailable (graceful degradation).\n   282→        \"\"\"\n   283→        if not self._client or not self._circuit_breaker:\n   284→            redis_logger.graceful_fallback(operation, \"Redis not initialized\")\n   285→            return None\n   286→\n   287→        if not await self._circuit_breaker.can_execute():\n   288→            redis_logger.graceful_fallback(operation, \"Circuit breaker open\")\n   289→            return None\n   290→\n   291→        start_time = time.monotonic()\n   292→        key = str(args[0]) if args else \"\"\n   293→\n   294→        try:\n   295→            method = getattr(self._client, operation)\n   296→            result = await method(*args, **kwargs)\n   297→\n   298→            duration_ms = (time.monotonic() - start_time) * 1000\n   299→            redis_logger.operation(operation, key, duration_ms, success=True)\n   300→            await self._circuit_breaker.record_success()\n   301→            return result\n   302→\n   303→        except RedisTimeoutError:\n   304→            duration_ms = (time.monotonic() - start_time) * 1000\n   305→            settings = get_settings()\n   306→            redis_logger.timeout(operation, key, settings.redis_socket_timeout)\n   307→            redis_logger.operation(operation, key, duration_ms, success=False)\n   308→            await self._circuit_breaker.record_failure()\n   309→            return None\n   310→\n   311→        except AuthenticationError as e:\n   312→            duration_ms = (time.monotonic() - start_time) * 1000\n   313→            settings = get_settings()\n   314→            redis_logger.connection_error(e, str(settings.redis_url))\n   315→   